                    <ReportItem id="RI1_2" type="EvalReviewCheckboxReportItem" blobType="BLOB_ARRAY" blobItem="EVL_EXCELLENCE_COM" blobSubItem="0"  subType="Project">
                    <![CDATA[
                    {$item}
                    ]]>
                    </ReportItem>
                </ReportItemSet>
                <ReportItem type="StaticReportItem">
