<?xml version="1.0" encoding="UTF-8" ?>
<Report name="Project Report" reportType="RP_PROGRESS" pdfType="RPTP_PROGRESS" personId="0" ajax="true" disabled="false">
    <ReportSection id="status" name="Header" type="HeaderReportSection" pagebreak="false" renderpdf="true">
        <ReportItem id="report_info_head" type="StaticReportItem">
            <![CDATA[
                <div style="float:left;"><h4 style='font-size:1.5em;'>Project Report {$this_year}</h4></div>
                <img class='logo' style="float:right;margin-top:0;position:static;" src='{$wgServer}{$wgScriptPath}/skins/{$networkName}_Logo.png' />
                <br />
                <br />
                <br />
                <br />
                <br />
            ]]>
        </ReportItem>
    </ReportSection>
    <ReportSection id="information" type="HeaderReportSection" name="Project Information" blobSection="INFORMATION">
        <ReportItem id="info" type="StaticReportItem">
            <![CDATA[
                <p><b>Theme:</b>           {$project_theme}</p>
                <p><b>Project Title:</b>   {$project_name}</p>
                <p><b>Project Leader:</b>  {$project_leaders}<br />
            ]]>
        </ReportItem>
        <ReportItemSet id="leaders" type="ProjectLeaders">
            <ReportItem id="leader_info">
                <![CDATA[
                    Location:       {$user_name}<br />
                    E-mail address: {$user_email}<br />
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItem id="co-investigators_table" type="StaticReportItem">
            <![CDATA[
                </p><br />
                <p><b>Co-Investigators:</b>
                <table class='wikitable' frame='box' rules='all' width='100%'>
                    <tr>
                        <th>Name</th>
                        <th>Location</th>
                        <th>E-mail address</th>
                    </tr>
            ]]>
        </ReportItem>
        <ReportItemSet id="co-investigators" type="ProjectPeopleNoLeaders">
            <ReportItem id="investigators_info" type="StaticReportItem">
                <![CDATA[
                    <tr>
                        <td>{$user_name}</td>
                        <td>{$user_uni}</td>
                        <td>{$user_email}</td>
                    </tr>
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItem id="hqp_table" type="StaticReportItem">
            <![CDATA[
                </table>
                </p><br />
                <p><b>Trainees and role:</b>
                <table class='wikitable' frame='box' rules='all' width='100%'>
                    <tr>
                        <th>Name</th>
                        <th>Location</th>
                        <th>Title</th>
                        <th>E-mail address</th>
                        <th>Supervisor(s)</th>
                    </tr>
            ]]>
        </ReportItem>
        <ReportItemSet id="hqps" type="ProjectAllPeopleReportItemSet" role="HQP">
            <ReportItem id="hqp_info" type="StaticReportItem">
                <![CDATA[
                    <tr>
                        <td>{$user_name}</td>
                        <td>{$user_uni}</td>
                        <td>{$user_level}</td>
                        <td>{$user_email}</td>
                        <td>{$user_supervisors}</td>
                    </tr>
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItem id="hqp_table" type="StaticReportItem">
            <![CDATA[
                </table>
                </p>
            ]]>
        </ReportItem>
        <ReportItem id="figures" type="UploadPDFReportItem" blobItem="FIGURES" fileSize="20" mimeType="application/pdf">
            <![CDATA[
                <p><b>Figures Upload</b></p>
                {$item}
            ]]>
        </ReportItem>
    </ReportSection>
    <ReportSection id="milestones" type="EditableReportSection" blobSection="MILESTONES" name="Milestones" title="Milestones">
        <ReportItem id="table" type="ProjectMilestoneTable">
            <![CDATA[
                {$item}<br />
            ]]>
        </ReportItem>
        <ReportItemSet id="is_pl" type="If" if="{contains({$my_full_roles},PL)}">
            <ReportItem id="comments" type="TextareaReportItem" blobItem="COMMENTS">
                <![CDATA[
                    <h3>Comments</h3>
                    {$item}
                ]]>
            </ReportItem>
        </ReportItemSet>
    </ReportSection>
    <ReportSection id="achievements" type="EditableReportSection" blobSection="ACHIEVEMENTS" name="Achievements" title="Research Achievements">
        <ReportItem id="achievements" type="TextareaReportItem" blobItem="ACHIEVEMENTS">
            <![CDATA[
                <p><b>Describe significant achievements or advances made in this project over the funding period</b></p>
                {$item}<br />
            ]]>
        </ReportItem>
        <ReportItem id="deliverables" type="TextareaReportItem" blobItem="DELIVERABLES">
            <![CDATA[
                <p><b>Cross-reference these achievements to deliverables and milestones set out in the original proposal.</b></p>
                {$item}<br />
            ]]>
        </ReportItem>
        <ReportItem id="impact" type="TextareaReportItem" blobItem="IMPACT">
            <![CDATA[
                <p><b>Outline the impact of these achievements in terms of the goals of the Project, of the theme and of GlycoNet.</b></p>
                {$item}<br />
            ]]>
        </ReportItem>
        <ReportItem id="benefits" type="TextareaReportItem" blobItem="BENEFITS">
            <![CDATA[
                <p><b>What are the benefits to Canada of the work to date?</b></p>
                {$item}<br />
            ]]>
        </ReportItem>
    </ReportSection>
    <ReportSection id="hqp" type="EditableReportSection" blobSection="HQP" name="HQP" title="Development of Highly Qualified Personnel">
        <ReportItemSet id="hqps" type="ProjectAllPeopleReportItemSet" role="HQP" startDate="{$this_year}-04-01 00:00:00" endDate="{$next_year}-04-15 00:00:00">
            <ReportItemSet id="header" type="ToggleHeaderReportItemSet" title="{$user_name}">
                <ReportItem id="roles" type="TextareaReportItem" blobItem="ROLES" blobSubItem="{$user_id}">
                    <![CDATA[
                        <p><b>Describe role of the trainee within the project</b></p>
                        {$item}<br />
                    ]]>
                </ReportItem>
                <ReportItem id="training" type="TextareaReportItem" blobItem="TRAINING" blobSubItem="{$user_id}">
                    <![CDATA[
                        <p><b>Describe any training initiatives undertaken during the funding period</b></p>
                        {$item}<br />
                    ]]>
                </ReportItem>
                <ReportItem id="graduate" type="TextareaReportItem" blobItem="GRADUATE" blobSubItem="{$user_id}">
                    <![CDATA[
                        <p><b>Did the trainee graduate or transition to the workforce? Describe.</b></p>
                        {$item}<br />
                    ]]>
                </ReportItem>
                <ReportItem id="disciplinary" type="TextareaReportItem" blobItem="DISCIPLINARY" blobSubItem="{$user_id}">
                    <![CDATA[
                        <p><b>How was cross-disciplinary training/experience achieved?</b></p>
                        {$item}<br />
                    ]]>
                </ReportItem>
                <ReportItem id="deliverables" type="TextareaReportItem" blobItem="DELIVERABLES" blobSubItem="{$user_id}">
                    <![CDATA[
                        <p><b>Cross-reference training initiative progress to deliverables included in the original proposal.</b></p>
                        {$item}<br />
                        <br />
                    ]]>
                </ReportItem>
            </ReportItemSet>
        </ReportItemSet>
    </ReportSection>
    <ReportSection id="networking" type="EditableReportSection" blobSection="NETWORKING" name="Networking" title="Networking and Partnerships">
        <ReportItem id="collaborations" type="TextareaReportItem" blobItem="COLLABORATIONS">
            <![CDATA[
                <p><b>Describe all collaborations and partners related to the project</b></p>
                {$item}<br />
            ]]>
        </ReportItem>
        <ReportItem id="resources" type="TextareaReportItem" blobItem="RESOURCES">
            <![CDATA[
                <p><b>Describe how resources and facilities have been optimised through collaborations.</b></p>
                {$item}<br />
            ]]>
        </ReportItem>
        <ReportItem id="accomplishment" type="TextareaReportItem" blobItem="ACCOMPLISHMENT">
            <![CDATA[
                <p><b>Cross-reference these accomplishment to deliverables and milestones included in the original proposal.</b></p>
                {$item}<br />
            ]]>
        </ReportItem>
    </ReportSection>
    <ReportSection id="ktee" type="EditableReportSection" blobSection="KTEE" name="KTEE" title="Knowledge exchange and technology exploitation">
        <ReportItem id="impact" type="TextareaReportItem" blobItem="IMPACT">
            <![CDATA[
                <p><b>Describe the impact or research outcomes of this project over the funding period that may result in the creation of intellectual property, outlicensing efforts, licenses negotiated or granted to industry, start-up companies created and similar entrepreneurial ventures.</b></p>
                {$item}<br />
            ]]>
        </ReportItem>
        <ReportItem id="project_dashboard_prod_details" type="DashboardReportItem" structure="PROJECT_REPORT_PRODUCTIVITY_STRUCTURE" table="false" details="true" totalOnly="true">
            <![CDATA[
                <h2>Productivity</h2>
                {$item}
            ]]>
        </ReportItem>
    </ReportSection>
    <ReportSection id="budget" type="EditableReportSection" blobSection="BUDGET" name="Budget" title="Budget Table and Justification">
        <ReportItem id="header" type="StaticReportItem">
            <![CDATA[
                <h3>Budget Justification</h3>
            ]]>
        </ReportItem>
        <ReportItemSet id="is_pl" type="If" if="{contains({$my_full_roles},PL)}">
            <ReportItem id="just" type="StaticReportItem">
                <![CDATA[
                    {set(just,)}
                ]]>
            </ReportItem>
            <ReportItemSet id="people" type="ProjectPeopleNoLeadersReportItemSet">
                <ReportItem id="comments" type="StaticReportItem">
                    <![CDATA[
                        {set(just,{concat({get(just)}, 
                            {concat(<h3>{$user_name}</h3>,
                                {getText(RP_PROGRESS,BUDGET,JUSTIFICATION,{$user_id},0,{$project_id})}
                            )}
                        )})}
                    ]]>
                </ReportItem>
            </ReportItemSet>
            <ReportItem id="impact_popup" type="PopupReportItem" title="NI Comments" text="{get(just)}">
                <![CDATA[
                    {$item}
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItem id="justification" type="TextareaReportItem" blobItem="JUSTIFICATION" blobSubItem="{$my_id}">
            <![CDATA[
                {$item}
            ]]>
        </ReportItem>
    </ReportSection>
    <ReportSection id="contributions" type="EditableReportSection" blobSection="CONTRIBUTIONS" name="Partner Contributions" title="Partner Contributions" pagebreak="false">
        <ReportItem id="project_dashboard_prod_details" type="DashboardReportItem" structure="PROJECT_CONTRIBUTION_STRUCTURE" table="false" details="true" showButton="false" totalOnly="true">
            <![CDATA[
                {$item}<br />
            ]]>
        </ReportItem>
        <ReportItem id="justification" type="TextareaReportItem" blobItem="JUSTIFICATION">
            <![CDATA[
                <h3>Justification</h3>
                {$item}
            ]]>
        </ReportItem>
    </ReportSection>
</Report>
