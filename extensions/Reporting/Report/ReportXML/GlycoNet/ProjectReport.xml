<?xml version="1.0" encoding="UTF-8" ?>
<Report name="Progress Report" reportType="RP_PROGRESS" pdfType="RPTP_PROGRESS" pdfFiles="ProjectReport" personId="0" ajax="true" disabled="false">
    <Permissions>
        <Role role="PL">
            <SectionPermission id="instructions" permissions="rw" />
            <SectionPermission id="milestones" permissions="rw" />
            <SectionPermission id="achievements" permissions="rw" />
            <SectionPermission id="hqp" permissions="rw" />
            <SectionPermission id="networking" permissions="rw" />
            <SectionPermission id="ktee" permissions="rw" />
            <SectionPermission id="budget" permissions="rw" />
            <SectionPermission id="contributions" permissions="rw" />
            <SectionPermission id="review" permissions="rw" />
        </Role>
        <Role role="NI">
            <SectionPermission id="instructions" permissions="rw" />
            <SectionPermission id="milestones" permissions="rw" />
            <SectionPermission id="achievements" permissions="rw" />
            <SectionPermission id="hqp" permissions="rw" />
            <SectionPermission id="networking" permissions="rw" />
            <SectionPermission id="ktee" permissions="rw" />
            <SectionPermission id="budget" permissions="rw" />
            <SectionPermission id="contributions" permissions="--" />
            <SectionPermission id="review" permissions="--" />
        </Role>
        <Role role="HQP">
            <SectionPermission id="instructions" permissions="rw" />
            <SectionPermission id="milestones" permissions="--" />
            <SectionPermission id="achievements" permissions="--" />
            <SectionPermission id="hqp" permissions="rw" />
            <SectionPermission id="networking" permissions="--" />
            <SectionPermission id="ktee" permissions="r" />
            <SectionPermission id="budget" permissions="--" />
            <SectionPermission id="contributions" permissions="--" />
            <SectionPermission id="review" permissions="--" />
        </Role>
    </Permissions>
    <ReportSection id="information" type="ReportSection" name="Project Information">
        <ReportItem id="info" type="StaticReportItem">
            <![CDATA[
                <p><b>Theme:</b>           {$project_theme}</p>
                <p><b>Project Title:</b>   {$project_name}</p>
                <p><b>Project Leader:</b>  {$project_leaders}<br />
            ]]>
        </ReportItem>
        <ReportItemSet id="leaders" type="ProjectLeaders">
            <ReportItem id="leader_info">
                <![CDATA[
                    Location:       {$user_name}<br />
                    E-mail address: {$user_email}<br />
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItem id="co-investigators_table" type="StaticReportItem">
            <![CDATA[
                </p>
                <p><b>Co-Investigators:</b>
                <table class='wikitable'>
                    <tr>
                        <th>Name</th>
                        <th>Location</th>
                        <th>E-mail address</th>
                    </tr>
            ]]>
        </ReportItem>
        <ReportItemSet id="co-investigators" type="ProjectPeopleNoLeaders">
            <ReportItem id="investigators_info" type="StaticReportItem">
                <![CDATA[
                    <tr>
                        <td>{$user_name}</td>
                        <td>{$user_uni}</td>
                        <td>{$user_email}</td>
                    </tr>
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItem id="hqp_table" type="StaticReportItem">
            <![CDATA[
                </table>
                </p>
                <p><b>Trainees and role:</b>
                <table class='wikitable'>
                    <tr>
                        <th>Name</th>
                        <th>Location</th>
                        <th>Title</th>
                        <th>E-mail address</th>
                        <th>Supervisor(s)</th>
                    </tr>
            ]]>
        </ReportItem>
        <ReportItemSet id="hqps" type="ProjectAllPeopleReportItemSet" role="HQP">
            <ReportItem id="hqp_info" type="StaticReportItem">
                <![CDATA[
                    <tr>
                        <td>{$user_name}</td>
                        <td>{$user_uni}</td>
                        <td>{$user_level}</td>
                        <td>{$user_email}</td>
                        <td>{$user_supervisors}</td>
                    </tr>
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItem id="hqp_table" type="StaticReportItem">
            <![CDATA[
                </table>
                </p>
            ]]>
        </ReportItem>
    </ReportSection>
    <ReportSection id="milestones" type="EditableReportSection" blobSection="MILESTONES" name="Milestones" title="Milestones">
        <ReportItem id="table" type="ProjectMilestoneTable">
            <![CDATA[
                {$item}<br />
                <a href='{$project_url}?tab=milestones&edit' class='button' target='_blank'>Edit Milestones</a>
            ]]>
        </ReportItem>
        <ReportItemSet id="is_pl" type="If" if="{contains({$my_full_roles},PL)}">
            <ReportItem id="comments" type="TextareaReportItem" blobItem="COMMENTS">
                <![CDATA[
                    <br /><br /><br /><br /><br />
                    <h3>Comments</h3>
                    <p>If any of the milestones have been delayed or abandoned, please comment on them below.</p>
                    {$item}
                ]]>
            </ReportItem>
        </ReportItemSet>
    </ReportSection>
    <ReportSection id="achievements" type="EditableReportSection" blobSection="ACHIEVEMENTS" name="Achievements" title="Research Achievements">
        <ReportItemSet id="is_pl" type="If" if="{contains({$my_full_roles},PL)}">
            <!-- Project Leader View -->
            <ReportItem id="comments" type="StaticReportItem">
                <![CDATA[
                    {set(achievements,)}
                    {set(deliverables,)}
                    {set(impact,)}
                    {set(benefits,)}
                ]]>
            </ReportItem>
            <ReportItemSet id="people" type="ProjectPeopleNoLeadersReportItemSet">
                <ReportItem id="comments" type="StaticReportItem">
                    <![CDATA[
                        {set(achievements,{concat({get(achievements)}, 
                            {concat(<h3>{$user_name}</h3>,
                                {getText(RP_PROGRESS,ACHIEVEMENTS,ACHIEVEMENTS,{$user_id},0,{$project_id})}
                            )}
                        )})}
                        
                        {set(deliverables,{concat({get(deliverables)}, 
                            {concat(<h3>{$user_name}</h3>,
                                {getText(RP_PROGRESS,ACHIEVEMENTS,DELIVERABLES,{$user_id},0,{$project_id})}
                            )}
                        )})}
                        
                        {set(impact,{concat({get(impact)}, 
                            {concat(<h3>{$user_name}</h3>,
                                {getText(RP_PROGRESS,ACHIEVEMENTS,IMPACT,{$user_id},0,{$project_id})}
                            )}
                        )})}
                        
                        {set(benefits,{concat({get(benefits)}, 
                            {concat(<h3>{$user_name}</h3>,
                                {getText(RP_PROGRESS,ACHIEVEMENTS,BENEFITS,{$user_id},0,{$project_id})}
                            )}
                        )})}
                    ]]>
                </ReportItem>
            </ReportItemSet>
            <ReportItem id="achievements_popup" type="PopupReportItem" title="NI Comments" text="{get(achievements)}">
                <![CDATA[
                    <p><b>Describe significant achievements or advances made in this project over the funding period</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="achievements" type="TextareaReportItem" blobItem="ACHIEVEMENTS">
                <![CDATA[
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="deliverables_popup" type="PopupReportItem" title="NI Comments" text="{get(deliverables)}">
                <![CDATA[
                    <p><b>Cross-reference these achievements to deliverables and milestones set out in the original proposal.</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="deliverables" type="TextareaReportItem" blobItem="DELIVERABLES">
                <![CDATA[
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="impact_popup" type="PopupReportItem" title="NI Comments" text="{get(impact)}">
                <![CDATA[
                    <p><b>Outline the impact of these achievements in terms of the goals of the Project, of the theme and of GlycoNet.</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="impact" type="TextareaReportItem" blobItem="IMPACT">
                <![CDATA[
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="benefits_popup" type="PopupReportItem" title="NI Comments" text="{get(benefits)}">
                <![CDATA[
                    <p><b>What are the benefits to Canada of the work to date?</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="benefits" type="TextareaReportItem" blobItem="BENEFITS">
                <![CDATA[
                    {$item}
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItemSet id="not_pl" type="If" if="{and({contains({$my_full_roles},CI)},{!contains({$my_full_roles},PL)})}">
            <!-- Network Investigator View -->
            <ReportItem id="achievements" type="TextareaReportItem" blobItem="ACHIEVEMENTS" blobSubItem="{$my_id}">
                <![CDATA[
                    <p><b>Describe significant achievements or advances made in this project over the funding period</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="deliverables" type="TextareaReportItem" blobItem="DELIVERABLES" blobSubItem="{$my_id}">
                <![CDATA[
                    <p><b>Cross-reference these achievements to deliverables and milestones set out in the original proposal.</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="impact" type="TextareaReportItem" blobItem="IMPACT" blobSubItem="{$my_id}">
                <![CDATA[
                    <p><b>Outline the impact of these achievements in terms of the goals of the Project, of the theme and of GlycoNet.</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="benefits" type="TextareaReportItem" blobItem="BENEFITS" blobSubItem="{$my_id}">
                <![CDATA[
                    <p><b>What are the benefits to Canada of the work to date?</b></p>
                    {$item}
                ]]>
            </ReportItem>
        </ReportItemSet>
    </ReportSection>
    <ReportSection id="hqp" type="EditableReportSection" blobSection="HQP" name="HQP" title="Development of Highly Qualified Personnel">
        <ReportItemSet id="is_pl" type="If" if="{contains({$my_full_roles},PL)}">
            <!-- Project Leader View -->
            <ReportItem id="note" type="StaticReportItem">
                <![CDATA[
                    <div class='info'>HQP responses may be edited by the Project Leader, Network Investigators, and by the HQP themselves.</div>
                ]]>
            </ReportItem>
            <ReportItemSet id="hqps" type="ProjectAllPeopleReportItemSet" role="HQP">
                <ReportItemSet id="header" type="ToggleHeaderReportItemSet" title="{$user_name}">
                    <ReportItem id="roles" type="TextareaReportItem" blobItem="ROLES" blobSubItem="{$user_id}">
                        <![CDATA[
                            <p><b>Describe role of the trainee within the project</b></p>
                            {$item}
                        ]]>
                    </ReportItem>
                    <ReportItem id="training" type="TextareaReportItem" blobItem="TRAINING" blobSubItem="{$user_id}">
                        <![CDATA[
                            <p><b>Describe any training initiatives undertaken during the funding period</b></p>
                            {$item}
                        ]]>
                    </ReportItem>
                    <ReportItem id="graduate" type="TextareaReportItem" blobItem="GRADUATE" blobSubItem="{$user_id}">
                        <![CDATA[
                            <p><b>Did the trainee graduate or transition to the workforce? Describe.</b>  <a href='{$user_url}?tab=hqp-alumni' target='_blank'>Update HQP Alumni</a></p>
                            {$item}
                        ]]>
                    </ReportItem>
                    <ReportItem id="disciplinary" type="TextareaReportItem" blobItem="DISCIPLINARY" blobSubItem="{$user_id}">
                        <![CDATA[
                            <p><b>How was cross-disciplinary training/experience achieved?</b></p>
                            {$item}
                        ]]>
                    </ReportItem>
                    <ReportItem id="deliverables" type="TextareaReportItem" blobItem="DELIVERABLES" blobSubItem="{$user_id}">
                        <![CDATA[
                            <p><b>Cross-reference training initiative progress to deliverables included in the original proposal.</b></p>
                            {$item}<br />
                            <br />
                        ]]>
                    </ReportItem>
                </ReportItemSet>
            </ReportItemSet>
        </ReportItemSet>
        <ReportItemSet id="not_pl" type="If" if="{and({contains({$my_full_roles},CI)},{!contains({$my_full_roles},PL)})}">
            <!-- Network Investigator View -->
            <ReportItemSet id="hqps" type="PersonSupervisesReportItemSet" project="{$project_name}" personId="{$my_id}">
                <ReportItemSet id="header" type="ToggleHeaderReportItemSet" title="{$user_name}">
                    <ReportItem id="roles" type="TextareaReportItem" blobItem="ROLES" blobSubItem="{$user_id}">
                        <![CDATA[
                            <p><b>Describe role of the trainee within the project</b></p>
                            {$item}
                        ]]>
                    </ReportItem>
                    <ReportItem id="training" type="TextareaReportItem" blobItem="TRAINING" blobSubItem="{$user_id}">
                        <![CDATA[
                            <p><b>Describe any training initiatives undertaken during the funding period</b></p>
                            {$item}
                        ]]>
                    </ReportItem>
                    <ReportItem id="graduate" type="TextareaReportItem" blobItem="GRADUATE" blobSubItem="{$user_id}">
                        <![CDATA[
                            <p><b>Did the trainee graduate or transition to the workforce? Describe.</b> <a href='{$user_url}?tab=hqp-alumni' target='_blank'>Update HQP Alumni</a></p>
                            {$item}
                        ]]>
                    </ReportItem>
                    <ReportItem id="disciplinary" type="TextareaReportItem" blobItem="DISCIPLINARY" blobSubItem="{$user_id}">
                        <![CDATA[
                            <p><b>How was cross-disciplinary training/experience achieved?</b></p>
                            {$item}
                        ]]>
                    </ReportItem>
                    <ReportItem id="deliverables" type="TextareaReportItem" blobItem="DELIVERABLES" blobSubItem="{$user_id}">
                        <![CDATA[
                            <p><b>Cross-reference training initiative progress to deliverables included in the original proposal.</b></p>
                            {$item}<br />
                            <br />
                        ]]>
                    </ReportItem>
                </ReportItemSet>
            </ReportItemSet>
        </ReportItemSet>
        <ReportItemSet id="not_pl" type="If" if="{and({contains({$my_full_roles},HQP)},{!contains({$my_full_roles},PL)})}">
            <!-- HQP View -->
            <ReportItem id="roles" type="TextareaReportItem" blobItem="ROLES" blobSubItem="{$my_id}">
                <![CDATA[
                    <div class='info'>Your responses may be edited by the Project Leader, and by the Network Investigators</div>
                    <p><b>Describe your role as a trainee within the project</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="training" type="TextareaReportItem" blobItem="TRAINING" blobSubItem="{$my_id}">
                <![CDATA[
                    <p><b>Describe any training initiatives undertaken during the funding period</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="graduate" type="TextareaReportItem" blobItem="GRADUATE" blobSubItem="{$my_id}">
                <![CDATA[
                    <p><b>Did you graduate or transition to the workforce? Describe.</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="disciplinary" type="TextareaReportItem" blobItem="DISCIPLINARY" blobSubItem="{$my_id}">
                <![CDATA[
                    <p><b>How was cross-disciplinary training/experience achieved?</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="deliverables" type="TextareaReportItem" blobItem="DELIVERABLES" blobSubItem="{$my_id}">
                <![CDATA[
                    <p><b>Cross-reference training initiative progress to deliverables included in the original proposal.</b></p>
                    {$item}
                ]]>
            </ReportItem>
        </ReportItemSet>
    </ReportSection>
    <ReportSection id="networking" type="EditableReportSection" blobSection="NETWORKING" name="Networking" title="Networking and Partnerships">
        <ReportItemSet id="is_pl" type="If" if="{contains({$my_full_roles},PL)}">
            <!-- Project Leader View -->
            <ReportItem id="comments" type="StaticReportItem">
                <![CDATA[
                    {set(collaborations,)}
                    {set(resources,)}
                    {set(accomplishment,)}
                ]]>
            </ReportItem>
            <ReportItemSet id="people" type="ProjectPeopleNoLeadersReportItemSet">
                <ReportItem id="comments" type="StaticReportItem">
                    <![CDATA[
                        {set(collaborations,{concat({get(collaborations)}, 
                            {concat(<h3>{$user_name}</h3>,
                                {getText(RP_PROGRESS,NETWORKING,COLLABORATIONS,{$user_id},0,{$project_id})}
                            )}
                        )})}
                        
                        {set(resources,{concat({get(resources)}, 
                            {concat(<h3>{$user_name}</h3>,
                                {getText(RP_PROGRESS,NETWORKING,RESOURCES,{$user_id},0,{$project_id})}
                            )}
                        )})}
                        
                        {set(accomplishment,{concat({get(accomplishment)}, 
                            {concat(<h3>{$user_name}</h3>,
                                {getText(RP_PROGRESS,NETWORKING,ACCOMPLISHMENT,{$user_id},0,{$project_id})}
                            )}
                        )})}
                    ]]>
                </ReportItem>
            </ReportItemSet>
            <ReportItem id="collaborations_popup" type="PopupReportItem" title="NI Comments" text="{get(collaborations)}">
                <![CDATA[
                    <p><b>Describe all collaborations and partners related to the project</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="collaborations" type="TextareaReportItem" blobItem="COLLABORATIONS">
                <![CDATA[
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="resources_popup" type="PopupReportItem" title="NI Comments" text="{get(resources)}">
                <![CDATA[
                    <p><b>Describe how resources and facilities have been optimised through collaborations.</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="resources" type="TextareaReportItem" blobItem="RESOURCES">
                <![CDATA[
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="accomplishment_popup" type="PopupReportItem" title="NI Comments" text="{get(accomplishment)}">
                <![CDATA[
                    <p><b>Cross-reference these accomplishment to deliverables and milestones included in the original proposal.</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="accomplishment" type="TextareaReportItem" blobItem="ACCOMPLISHMENT">
                <![CDATA[
                    {$item}
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItemSet id="not_pl" type="If" if="{and({contains({$my_full_roles},CI)},{!contains({$my_full_roles},PL)})}">
            <!-- Network Investigator View -->
            <ReportItem id="collaborations" type="TextareaReportItem" blobItem="COLLABORATIONS" blobSubItem="{$my_id}">
                <![CDATA[
                    <p><b>Describe all collaborations and partners related to the project</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="resources" type="TextareaReportItem" blobItem="RESOURCES" blobSubItem="{$my_id}">
                <![CDATA[
                    <p><b>Describe how resources and facilities have been optimised through collaborations.</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="accomplishment" type="TextareaReportItem" blobItem="ACCOMPLISHMENT" blobSubItem="{$my_id}">
                <![CDATA[
                    <p><b>Cross-reference these accomplishment to deliverables and milestones included in the original proposal.</b></p>
                    {$item}
                ]]>
            </ReportItem>
        </ReportItemSet>
    </ReportSection>
    <ReportSection id="ktee" type="EditableReportSection" blobSection="KTEE" name="KTEE" title="Knowledge exchange and technology exploitation">
        <ReportItemSet id="is_pl" type="If" if="{contains({$my_full_roles},PL)}">
            <!-- Project Leader View -->
            <ReportItem id="comments" type="StaticReportItem">
                <![CDATA[
                    {set(impact,)}
                ]]>
            </ReportItem>
            <ReportItemSet id="people" type="ProjectPeopleNoLeadersReportItemSet">
                <ReportItem id="comments" type="StaticReportItem">
                    <![CDATA[
                        {set(impact,{concat({get(impact)}, 
                            {concat(<h3>{$user_name}</h3>,
                                {getText(RP_PROGRESS,KTEE,IMPACT,{$user_id},0,{$project_id})}
                            )}
                        )})}
                    ]]>
                </ReportItem>
            </ReportItemSet>
            <ReportItem id="impact_popup" type="PopupReportItem" title="NI Comments" text="{get(impact)}">
                <![CDATA[
                    <p><b>Describe the impact or research outcomes of this project over the funding period that may result in the creation of intellectual property, outlicensing efforts, licenses negotiated or granted to industry, start-up companies created and similar entrepreneurial ventures.</b></p>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="impact" type="TextareaReportItem" blobItem="IMPACT">
                <![CDATA[
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="figures" type="UploadReportItem" blobItem="FIGURES" fileSize="10" mimeType="application/pdf">
                <![CDATA[
                    <p><b>Figures Upload</b></p>
                    {$item}
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItemSet id="not_pl" type="If" if="{and({contains({$my_full_roles},CI)},{!contains({$my_full_roles},PL)})}">
            <!-- Network Investigator View -->
            <ReportItem id="impact" type="TextareaReportItem" blobItem="IMPACT" blobSubItem="{$my_id}">
                <![CDATA[
                    <p><b>Describe the impact or research outcomes of this project over the funding period that may result in the creation of intellectual property, outlicensing efforts, licenses negotiated or granted to industry, start-up companies created and similar entrepreneurial ventures.</b></p>
                    {$item}
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItem id="project_dashboard_prod_details" type="DashboardReportItem" structure="PROJECT_REPORT_PRODUCTIVITY_STRUCTURE" table="false" details="true" totalOnly="true">
            <![CDATA[
                <h2>Productivity</h2>
                <p><b>List abstracts and publications (peer-reviewed or not) that resulted from the outcomes of work during the funding period.</b></p>
                <p>To add products (Publications, Abstracts, Patents etc.), go to <a href='{$wgServer}{$wgScriptPath}/index.php/Special:ManageProducts' target='_blank'>Manage Products</a>.  In order for the product to show up below, it must be associated with the project, and have been written between the reporting period (April 1, {$this_year} - March 31, {$next_year})</p>
                {$item}
            ]]>
        </ReportItem>
    </ReportSection>
    <ReportSection id="budget" type="EditableReportSection" blobSection="BUDGET" name="Budget" title="Budget Table and Justification">
        <ReportItem id="header" type="StaticReportItem">
            <![CDATA[
                <h3>Budget Justification</h3>
                <p><b>Provide a written justification for any significant (>20%) deviation from the original budget in your approved proposal.</b></p>
                <p>Provide details of all stipends paid, including names, positions and amounts paid.</p>
            ]]>
        </ReportItem>
        <ReportItemSet id="is_pl" type="If" if="{contains({$my_full_roles},PL)}">
            <ReportItem id="just" type="StaticReportItem">
                <![CDATA[
                    {set(just,)}
                ]]>
            </ReportItem>
            <ReportItemSet id="people" type="ProjectPeopleNoLeadersReportItemSet">
                <ReportItem id="comments" type="StaticReportItem">
                    <![CDATA[
                        {set(just,{concat({get(just)}, 
                            {concat(<h3>{$user_name}</h3>,
                                {getText(RP_PROGRESS,BUDGET,JUSTIFICATION,{$user_id},0,{$project_id})}
                            )}
                        )})}
                    ]]>
                </ReportItem>
            </ReportItemSet>
            <ReportItem id="impact_popup" type="PopupReportItem" title="NI Comments" text="{get(just)}">
                <![CDATA[
                    {$item}
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItem id="justification" type="TextareaReportItem" blobItem="JUSTIFICATION" blobSubItem="{$my_id}">
            <![CDATA[
                {$item}
            ]]>
        </ReportItem>
    </ReportSection>
    <ReportSection id="contributions" type="EditableReportSection" blobSection="CONTRIBUTIONS" name="Partner Contributions" title="Partner Contributions">
        <ReportItem id="project_dashboard_prod_details" type="DashboardReportItem" structure="PROJECT_CONTRIBUTION_STRUCTURE" table="true" details="true" showButton="false" totalOnly="true">
            <![CDATA[
                {$item}<br />
                <a class='button' href='{$wgServer}{$wgScriptPath}/index.php/Special:AddContributionPage' target='_blank'>Add/Edit Contributions</a>
            ]]>
        </ReportItem>
        <ReportItem id="justification" type="TextareaReportItem" blobItem="JUSTIFICATION">
            <![CDATA[
                <h3>Justification</h3>
                <p><b>Provide a written explanation of any deviation between the actual contributions and anticipated expenditures and those proposed in the original approved proposal.</b></p>
                {$item}
            ]]>
        </ReportItem>
    </ReportSection>
    <ReportSection id="review" tooltip="Review of on-line report content" name="Review" blobSection="SEC_NONE" type="ReportSection" renderpdf="false">
        <ReportItem id="report_review" type="ReviewReportItem">
            <![CDATA[{$item}]]>
        </ReportItem>
    </ReportSection>
</Report>
