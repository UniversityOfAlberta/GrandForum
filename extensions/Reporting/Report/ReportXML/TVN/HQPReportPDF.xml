<?xml version="1.0" encoding="UTF-8" ?>
<Report name="HQP Report" reportType="RP_SSA_FINAL_PROGRESS" pdfType="RPTP_SSA_FINAL_REPORT" ajax="true">
    <ReportSection id="status" name="Header" type="HeaderReportSection" pagebreak="false" renderpdf="true">
        <ReportItem id="report_info_head" type="StaticReportItem">
            <![CDATA[
                    <img class='logo' style="position:static; float:right;margin-top:0;" src='{$wgServer}{$wgScriptPath}/skins/{$networkName}_Logo.png' />
                    <h4 style='font-size:1.40em;'>{$this_year} HQP Report</h4><br />
                    <br />
                    <br />
                    <br />
                    <br />
                    <br />
                    <br />
                    <br />
            ]]>
        </ReportItem>
    </ReportSection>
    <ReportSection id="hqp" type="HeaderReportSection" blobSection="SSA_HQP" name="HQP Trainees">
        <ReportItem id="bookmark" type="BookmarkReportItem" title="HQP Trainees">
            <![CDATA[
                {$item}
            ]]>
        </ReportItem>
        <ReportItemSet id="students" type="PersonSupervisesReportItemSet">
            <ReportItemSet id="notfirst" type="NotFirstReportItemSet">
                <ReportItem id="pagebreak" type="PageBreakReportItem">
                    <![CDATA[
                        {$item}
                    ]]>
                </ReportItem>
            </ReportItemSet>
            <ReportItemSet id="header" type="ToggleHeaderReportItemSet" title="{$user_name}" level="2">
                <ReportItem id="bookmark" type="SubBookmarkReportItem" title="{$user_name}">
                    <![CDATA[
                        {$item}
                    ]]>
                </ReportItem>
                <ReportItem id="demographic_head" type="StaticReportItem">
                    <![CDATA[
                        <h3>Demographic Information</h3>
                        <table class='wikitable'>
                    ]]>
                </ReportItem>
                <ReportItem id="grant" type="TextReportItem" width="300px" blobType="BLOB_TEXT" blobItem="SSA_HQP_GRANT" blobSubItem="{$user_id}">
                    <![CDATA[
                        <tr>
                            <td class='label'>Grant Title:</td>
                            <td>{$item}</td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="uni" type="StaticReportItem">
                    <![CDATA[
                        <tr>
                            <td class='label'>University Attended:</td>
                            <td>{$user_uni}</td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="category" type="StaticReportItem">
                    <![CDATA[
                        <tr>
                            <td class='label'>HQP Category:</td>
                            <td>{$user_level}</td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="gender" type="StaticReportItem">
                    <![CDATA[
                        <tr>
                            <td class='label'>Gender:</td>
                            <td>{$user_gender}</td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="start" type="CalendarReportItem" width="80px" blobType="BLOB_TEXT" blobItem="SSA_HQP_START" blobSubItem="{$user_id}">
                    <![CDATA[
                        <tr>
                            <td class='label'>Start Date:</td>
                            <td>{$item}</td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="nationality" type="StaticReportItem">
                    <![CDATA[
                        <tr>
                            <td class='label'>Nationality:</td>
                            <td>{$user_nationality}</td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="trainee_term" type="IntegerReportItem" blobType="BLOB_TEXT" blobItem="SSA_HQP_TERM" blobSubItem="{$user_id}" min="0" max="100" width="30px">
                    <![CDATA[
                        <tr>
                            <td class='label'>Trainee Term:</td>
                            <td>{$item} months</td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="trainee_term" type="IntegerReportItem" blobType="BLOB_TEXT" blobItem="SSA_HQP_COMPLETED" blobSubItem="{$user_id}" min="0" max="100" width="30px">
                    <![CDATA[
                        <tr>
                            <td class='label'>Term Completed:</td>
                            <td>{$item} months</td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="additional_head" type="StaticReportItem">
                    <![CDATA[
                        </table><br />
                        <h3>Publications and Dissemination of Research</h3>
                    ]]>
                </ReportItem>
                <ReportItem id="dashboard" type="DashboardReportItem" structure="HQP_REPORT_STRUCTURE" table="false" details="true" totalOnly="true">
                    <![CDATA[
                        {$item}
                    ]]>
                </ReportItem>
                <ReportItem id="additional_head" type="StaticReportItem">
                    <![CDATA[
                        </table><br />
                        <h3>Additional Information</h3>
                        <table class='wikitable'>
                    ]]>
                </ReportItem>
                <ReportItem id="training" type="RadioReportItem" blobType="BLOB_ARRAY" blobItem="SSA_HQP_TRAIN" blobSubItem="{$user_id}" options="Yes|No">
                    <![CDATA[
                        <tr>
                            <td class='label'>Participating in the TVN Training Program?</td>
                            <td>{$item}</td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="experiences" type="RadioReportItem" blobType="BLOB_ARRAY" blobItem="SSA_HQP_EXP" blobSubItem="{$user_id}" options="Yes|No">
                    <![CDATA[
                        <tr>
                            <td class='label'>Other Training Experiences provided:</td>
                            <td>{$item}</td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="thesis" type="RadioReportItem" blobType="BLOB_ARRAY" blobItem="SSA_HQP_THESIS" blobSubItem="{$user_id}" options="Yes|No">
                    <![CDATA[
                        <tr>
                            <td class='label'>Thesis Completed:</td>
                            <td>{$item}</td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="grad" type="RadioReportItem" blobType="BLOB_ARRAY" blobItem="SSA_HQP_GRAD" blobSubItem="{$user_id}" options="Yes|No">
                    <![CDATA[
                        <tr>
                            <td class='label'>Did The Student Graduate?</td>
                            <td>{$item}</td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="empl" type="RadioReportItem" blobType="BLOB_ARRAY" blobItem="SSA_HQP_EMPL" blobSubItem="{$user_id}" options="Yes|No">
                    <![CDATA[
                        <tr>
                            <td class='label'>Did The Student Gain Employment?</td>
                            <td>{$item}</td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="where" type="TextReportItem" blobType="BLOB_TEXT" blobItem="SSA_HQP_WHERE" blobSubItem="{$user_id}">
                    <![CDATA[
                        <tr>
                            <td class='label'><small>If yes, please indicate where they were employed?</small></td>
                            <td>{$item}</td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="additional_foot" type="StaticReportItem">
                    <![CDATA[
                        </table><br />
                    ]]>
                </ReportItem>
            </ReportItemSet>
        </ReportItemSet>
    </ReportSection>
    <ReportSection id="report" type="EditableReportSection" blobSection="SSA_REPORT" name="SSA Report" pagebreak="false">
        <ReportItemSet id="students" type="PersonSupervisesReportItemSet" pos="Summer Student">
            <ReportItemSet id="notfirst" type="NotFirstReportItemSet">
                <ReportItem id="pagebreak" type="PageBreakReportItem">
                    <![CDATA[
                        {$item}
                    ]]>
                </ReportItem>
            </ReportItemSet>
            <ReportItemSet id="header" type="ToggleHeaderReportItemSet" title="{$user_name}" level="2">
                <ReportItem id="bookmark" type="SubBookmarkReportItem" title="{$user_name}">
                    <![CDATA[
                        {$item}
                    ]]>
                </ReportItem>
                <ReportItem id="identity_head" type="StaticReportItem">
                    <![CDATA[
                        <small><table class='wikitable' frame='box' rules='all' width="100%">
                            <tr>
                                <td><b>Student Last Name</b></td>
                                <td style='white-space:nowrap;'>{$user_last_name}</td>
                                <td><b>Student First Name</b></td>
                                <td style='white-space:nowrap;'>{$user_first_name}</td>
                            </tr>
                            <tr>
                                <td><b>Supervisor Last Name</b></td>
                                <td style='white-space:nowrap;'>{$my_last_name}</td>
                                <td><b>Supervisor First Name</b></td>
                                <td style='white-space:nowrap;'>{$my_first_name}</td>
                            </tr>
                        
                    ]]>
                </ReportItem>
                <ReportItem id="start" type="CalendarReportItem" blobType="BLOB_TEXT" blobItem="SSA_START" blobSubItem="{$user_id}">
                    <![CDATA[
                        <tr>
                            <td><b>Date student started working</b></td>
                            <td style='white-space:nowrap;'>{$item}</td>
                    ]]>
                </ReportItem>
                <ReportItem id="end" type="CalendarReportItem" blobType="BLOB_TEXT" blobItem="SSA_END" blobSubItem="{$user_id}">
                    <![CDATA[
                            <td><b>Date student finished working</b></td>
                            <td style='white-space:nowrap;'>{$item}</td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="fulltime" type="RadioReportItem" options="Yes|No" orientation="horizontal" blobType="BLOB_TEXT" blobItem="SSA_TIME" blobSubItem="{$user_id}">
                    <![CDATA[
                        <tr>
                            <td><b>Did student work full time on the funded activities during this period?</b></td>
                            <td style='white-space:nowrap;'>{$item}<br />
                    ]]>
                </ReportItem>
                <ReportItem id="fulltime_no" type="TextReportItem" blobType="BLOB_TEXT" blobItem="SSA_NO" blobSubItem="{$user_id}" optional="true">
                    <![CDATA[
                            If no, # hours per week?<br />
                            {$item}
                        </td>
                    ]]>
                </ReportItem>
                <ReportItem id="another" type="RadioReportItem" options="Yes|No" orientation="horizontal" blobType="BLOB_TEXT" blobItem="SSA_ANOTHER" blobSubItem="{$user_id}">
                    <![CDATA[
                            <td><b>Would you supervise another summer student in future?</b></td>
                            <td style='white-space:nowrap;'>{$item}</td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="identity_foot" type="StaticReportItem">
                    <![CDATA[
                        </table></small><br />
                    ]]>
                </ReportItem>
                <ReportItem id="achievements" type="TextareaReportItem" rich="true" height="400px" blobType="BLOB_TEXT" blobItem="SSA_ACHIEVEMENTS" blobSubItem="{$user_id}">
                    <![CDATA[
                        <h3>Describe the student’s major research activities and related key achievements over this reporting period and if applicable, emphasize collaborative achievements.</h3>
                        {$item}
                    ]]>
                </ReportItem>
                <ReportItem id="assessment_head" type="StaticReportItem">
                    <![CDATA[
                        <h3>Please assess your student’s activities/project against the following</h3>
                        <table class='wikitable' rules="all" box="all" width="100%">
                    ]]>
                </ReportItem>
                <ReportItem id="priorities" type="RadioReportItem" options="Matching care to values|Empowering, engaging and supporting patients and their families/caregivers|Improving clinical outcomes|Improving care across the continuum" orientation="vertical" blobType="BLOB_TEXT" blobItem="SSA_PRIORITIES" blobSubItem="{$user_id}">
                    <![CDATA[
                        <tr>
                            <td width="50%" valign='top'>
                                <b>TVN Strategic Priorities</b>
                                <div style='margin-left:10px;'>{$item}</div>
                            </td>
                    ]]>
                </ReportItem>
                <ReportItem id="themes" type="RadioReportItem" options="Improved end-of-life care/advance care planning|Improved acute/critical care|Optimization of community/residential care|Optimization of transitions of care" orientation="vertical" blobType="BLOB_TEXT" blobItem="SSA_THEMES" blobSubItem="{$user_id}">
                    <![CDATA[
                            <td width="50%" valign='top'>
                                <b>TVN Research Themes</b>
                                <div style='margin-left:10px;'>{$item}</div>
                            </td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="care" type="RadioReportItem" options="Palliative/EOL care|Acute/critical care in hospital|Long-term care facility|Home care|All settings of care" orientation="vertical" blobType="BLOB_TEXT" blobItem="SSA_CARE" blobSubItem="{$user_id}">
                    <![CDATA[
                        <tr>
                            <td valign='top'>
                                <b>Setting of Care</b>
                                <div style='margin-left:10px;'>{$item}</div>
                            </td>
                    ]]>
                </ReportItem>
                <ReportItem id="impact" type="RadioReportItem" options="Patients, their families and informal caregivers/support systems (volunteer)|Health care professionals and caregivers (paid)|Health care facilities|Health care system" orientation="vertical" blobType="BLOB_TEXT" blobItem="SSA_IMPACT" blobSubItem="{$user_id}">
                    <![CDATA[
                            <td valign='top'>
                                <b>Focus of Impact</b>
                                <div style='margin-left:10px;'>{$item}</div>
                            </td>
                        </tr>
                    ]]>
                </ReportItem>
                <ReportItem id="synth" type="TextReportItem" width="50px" blobType="BLOB_TEXT" blobItem="SSA_SYNTH" blobSubItem="{$user_id}">
                    <![CDATA[
                        </table><br />
                        <h4>Knowledge Activities</h4>
                        {$item}% Knowledge synthesis<br />
                    ]]>
                </ReportItem>
                <ReportItem id="creation" type="TextReportItem" width="50px" blobType="BLOB_TEXT" blobItem="SSA_CREATION" blobSubItem="{$user_id}">
                    <![CDATA[
                        {$item}% Knowledge creation<br />
                    ]]>
                </ReportItem>
                <ReportItem id="mobilization" type="TextReportItem" width="50px" blobType="BLOB_TEXT" blobItem="SSA_MOBILIZATION" blobSubItem="{$user_id}">
                    <![CDATA[
                        {$item}% Knowledge mobilization<br /><br />
                    ]]>
                </ReportItem>
                <ReportItem id="institution" type="UploadReportItem" blobType="BLOB_RAW" blobItem="SSA_INSTITUTION" mimeType="application/pdf" fileSize="2" blobSubItem="{$user_id}">
                    <![CDATA[
                        <h4>Host Institution</h4>
                        {$item}
                    ]]>
                </ReportItem>
            </ReportItemSet>
        </ReportItemSet>
    </ReportSection>
</Report>
