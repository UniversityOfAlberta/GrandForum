<?xml version="1.0" encoding="UTF-8" ?>
<Report name="FEC" reportType="RP_FEC" pdfType="RPTP_FEC" headerName="{$user_name}" ajax="true" encrypt="true">
    <ReportSection id="information" tooltip="Information" name="Information" title="Annual Report of Faculty Member - {$networkName}" blobSection="FEC_INFORMATION" type="EditableReportSection" pagebreak="true">
        <Static>
            <![CDATA[
                <h3 style='margin-top: -0.5em;'>Period: {time2date(CYCLE_START, F Y)} to {time2date(CYCLE_END, F Y)}</h3>
                <table class="nospacing" style="line-height:1.75em; width: 100%;" cellpadding='0' cellspacing='0'>
                    <tr>
                        <td valign='top'>
                            <table width="100%" class='wikitable nospacing' cellpadding='0' cellspacing='0'>
                                <tr><td><span class='header'>Name:</span>&nbsp;<b>{$user_name}</b></td></tr>
                                <tr><td><span class='header'>Rank:</span>&nbsp;<b>{$user_level}</b></td></tr>
                                <tr><td><span class='header'>Department(s):</span>&nbsp;<b>{$user_depts}</b></td></tr>
                                <tr><td><span class='header'>Graduate Students(Supervised or Co-supervised):</span>&nbsp;<b>{$user_grad_count}</td></b></tr>
                                <tr><td><span class='header'>Post-doctoral Fellows and Research Associates (Supervised or Co-supervised):</span>&nbsp;<b>{$user_fellow_count}</b></td></tr>
                                <tr><td><span class='header'>Research/Technical Assistants:</span>&nbsp;<b>{$user_tech_count}</b></td></tr>
                                <tr><td><span class='header'>Undergraduates (Supervised or Co-supervised):</span>&nbsp;<b>{$user_ugrad_count}</b></td></tr>
                                <tr><td><span class='header'>Refereed Publications:&nbsp;</span><b>{getUserPublicationCount(CYCLE_START, CYCLE_END, Publication)}</b></td></tr>
                                <tr><td><span class='header'>Books:&nbsp;</span><b>{getUserPublicationCount(CYCLE_START, CYCLE_END, Book)}</b></td></tr>
                                <tr><td><span class='header'>Patents:&nbsp;</span><b>{getUserPublicationCount(CYCLE_START, CYCLE_END, Patent)}</b></td></tr>
                            </table>
                        </td>
                        <td valign='top' align='right'>
                            <table class='wikitable nospacing' cellpadding='0' cellspacing='0'>
                                {if("{$user_phd_year}" != "", <tr><td><span class='header'>Year of PhD:&nbsp;</span></td><td><b>{$user_phd_year}</b></td></tr>)}
                                {if("{$user_appointment_year}" != "", <tr><td><span class='header'>Year of Appointment:&nbsp;</span></td><td><b>{$user_appointment_year}</b></td></tr>)}
                                {if("{getFecPersonalInfo(dateOfAssistant)}" != "", <tr><td><span class='header'>Date of Assistant:&nbsp;</span></td><td><b>{getFecPersonalInfo(dateOfAssistant)}</b></td></tr>)}
                                {if("{getFecPersonalInfo(dateOfAssociate)}" != "", <tr><td><span class='header'>Date of Associate:&nbsp;</span></td><td><b>{getFecPersonalInfo(dateOfAssociate)}</b></td></tr>)}
                                {if("{getFecPersonalInfo(dateOfProfessor)}" != "", <tr><td><span class='header'>Date of Professor:&nbsp;</span></td><td><b>{getFecPersonalInfo(dateOfProfessor)}</b></td></tr>)}
                                {if("{getFecPersonalInfo(dateFso2)}" != "", <tr><td><span class='header'>Date of FSO II:&nbsp;</span></td><td><b>{getFecPersonalInfo(dateFso2)}</b></td></tr>)}
                                {if("{getFecPersonalInfo(dateFso3)}" != "", <tr><td><span class='header'>Date of FSO III:&nbsp;</span></td><td><b>{getFecPersonalInfo(dateFso3)}</b></td></tr>)}
                                {if("{getFecPersonalInfo(dateFso4)}" != "", <tr><td><span class='header'>Date of FSO IV:&nbsp;</span></td><td><b>{getFecPersonalInfo(dateFso4)}</b></td></tr>)}
                                {if("{getFecPersonalInfo(dateAtsec1)}" != "", <tr><td><span class='header'>Date of ATS I:&nbsp;</span></td><td><b>{getFecPersonalInfo(dateAtsec1)}</b></td></tr>)}
                                {if("{getFecPersonalInfo(dateAtsec2)}" != "", <tr><td><span class='header'>Date of ATS II:&nbsp;</span></td><td><b>{getFecPersonalInfo(dateAtsec2)}</b></td></tr>)}
                                {if("{getFecPersonalInfo(dateAtsec3)}" != "", <tr><td><span class='header'>Date of ATS III:&nbsp;</span></td><td><b>{getFecPersonalInfo(dateAtsec3)}</b></td></tr>)}
                                {if("{getFecPersonalInfo(dateOfProbation1)}" != "", <tr><td><span class='header'>Date of Probation 1:&nbsp;</span></td><td><b>{getFecPersonalInfo(dateOfProbation1)}</b></td></tr>)}
                                {if("{getFecPersonalInfo(dateOfProbation2)}" != "", <tr><td><span class='header'>Date of Probation 2:&nbsp;</span></td><td><b>{getFecPersonalInfo(dateOfProbation2)}</b></td></tr>)}
                                {if("{getFecPersonalInfo(dateOfTenure)}" != "", <tr><td><span class='header'>Date of Tenure:&nbsp;</span></td><td><b>{getFecPersonalInfo(dateOfTenure)}</b></td></tr>)}
                                {if("{getFecPersonalInfo(dateOfRetirement)}" != "", <tr><td><span class='header'>Date of Retirement:&nbsp;</span></td><td><b>{getFecPersonalInfo(dateOfRetirement)}</b></td></tr>)}
                                {if("{getFecPersonalInfo(dateOfLastDegree)}" != "", <tr><td><span class='header'>Date of Last Degree:&nbsp;</span></td><td><b>{getFecPersonalInfo(dateOfLastDegree)}</b></td></tr>)}
                                {if("{getFecPersonalInfo(lastDegree)}" != "", <tr><td><span class='header'>Last Degree:&nbsp;</span></td><td><b>{getFecPersonalInfo(lastDegree, false)}</b></td></tr>)}
                            </table>
                        </td>
                    </tr>
                </table><br />
                <h3>Keywords</h3>
                {$user_keywords}<br />
            ]]>
        </Static>
        
        <!-- Responsibilities Variation -->
        <Static>
            <![CDATA[
                <h2>Responsibilities Variation</h2>
            ]]>
        </Static>
        <If if="{==({getText(RP_LETTER5, TABLE, TEMPLATE, {$user_id}, 1, 0, {$last_year})},Variance)}">
            <If if="{==({getText(RP_LETTER5, TABLE, REPLACE_ROLE, {$user_id}, 1, 0, {$last_year})},Yes)}">
                <![CDATA[
                    <p>In accordance with Article A2.01.2, I am writing to confirm the following proportion of University responsibilities for the annual review period[s] of {time2date({getText(RP_LETTER5, TABLE, START_DATE, {$user_id}, 1, 0, {$last_year})}, F Y)} to {time2date({getText(RP_LETTER5, TABLE, END_DATE, {$user_id}, 1, 0, {$last_year})}, F Y)}, {getText(RP_LETTER5, TABLE, ROLE, {$user_id}, 1, 0, {$last_year})}.</p>
                ]]>
            </If>
            <ElseIf if="{!=({getText(RP_LETTER5, TABLE, ROLE, {$user_id}, 1, 0, {$last_year})},)}">
                <![CDATA[
                    <p>In accordance with Article A2.01.2, I am writing to confirm the following proportion of University responsibilities for the annual review period[s] of {time2date({getText(RP_LETTER5, TABLE, START_DATE, {$user_id}, 1, 0, {$last_year})}, F Y)} to {time2date({getText(RP_LETTER5, TABLE, END_DATE, {$user_id}, 1, 0, {$last_year})}, F Y)}, during which period you will be holding the role of {getText(RP_LETTER5, TABLE, ROLE, {$user_id}, 1, 0, {$last_year})}.</p>
                ]]>
            </ElseIf>
            <Else>
                <![CDATA[
                    <p>In accordance with Article A2.01.2, I am writing to confirm the following proportion of University responsibilities for the annual review period[s] of {time2date({getText(RP_LETTER5, TABLE, START_DATE, {$user_id}, 1, 0, {$last_year})}, F Y)} to {time2date({getText(RP_LETTER5, TABLE, END_DATE, {$user_id}, 1, 0, {$last_year})}, F Y)}.</p>
                ]]>
            </Else>
            <Static>
                <![CDATA[
                    <table>
                        <tr><td align='right'><b>Teaching:</b></td><td align='right' style='width:3em;'>{getText(RP_LETTER5, TABLE, TEACHING, {$user_id}, 1, 0, {$last_year})}%</td></tr>
                        <tr><td align='right'><b>Research:</b></td><td align='right' style='width:3em;'>{getText(RP_LETTER5, TABLE, RESEARCH, {$user_id}, 1, 0, {$last_year})}%</td></tr>
                        <tr><td align='right'><b>Service:</b></td><td align='right' style='width:3em;'>{getText(RP_LETTER5, TABLE, SERVICE, {$user_id}, 1, 0, {$last_year})}%</td></tr>
                        <tr><td align='right'><b>Admin:</b></td><td align='right' style='width:3em;'>{getText(RP_LETTER5, TABLE, ADMIN, {$user_id}, 1, 0, {$last_year})}%</td></tr>
                    </table>
                ]]>
            </Static>
        </If>
        <Else>
            <![CDATA[
                <table>
                    <tr><td align='right'><b>Teaching:</b></td><td align='right' style='width:3em;'>40%</td></tr>
                    <tr><td align='right'><b>Research:</b></td><td align='right' style='width:3em;'>40%</td></tr>
                    <tr><td align='right'><b>Service:</b></td><td align='right' style='width:3em;'>20%</td></tr>
                    <tr><td align='right'><b>Admin:</b></td><td align='right' style='width:3em;'>0%</td></tr>
                </table>
            ]]>
        </Else>
        <Static>
            <![CDATA[
                <p>If this is not correct, please discuss the issue with your chair and work together to develop an appropriate “Variances of Responsibility” memo</p>
            ]]>
        </Static>
        
        <!-- Leaves -->
        <ReportItem id="leaves" type="MultiTextReportItem" blobItem="FEC_INFO_LEAVES" blobType="BLOB_ARRAY" labels="Category|Category Description(if other)|Start Date|End Date" types="Text|Date(yy/mm)|Date(yy/mm)" multiple="true" optional="true" class="wikitable">
            <![CDATA[
                </div>
                <h3>Leaves</h3>
                {$item}
            ]]>
        </ReportItem>
        <ReportItem id="reasons" type="MultiTextReportItem" blobItem="FEC_INFO_REASONS" blobType="BLOB_ARRAY" labels="Reason|Reason(if other)|Comments" types="Select(Administrative Leave,Assisted Leave,Childbirth Leave,Compassionate Leave,Disability Leave,Leave without Pay,Medical Leave,Parental Leave,Political Leave,Sabbatical Leave,Associate Chair/Dean,Cross Appointment,Departmental Chair,Fellow/Scholar/Award,New Faculty,Research/Teaching Chair,Secondment,Other)|Text|Textarea" class="wikitable" multiple="true" optional="true">
            <![CDATA[
                <h3>Reasons For Reduced Teaching<small> (if applicable)</small></h3>
                {$item}
            ]]>
        </ReportItem>
        
        <!-- Courses -->
        <ReportItem id="coursesbookmark" type="BookmarkReportItem" title="Courses">
            <![CDATA[
                <h2>Courses</h2>
            ]]>
        </ReportItem>
        
        <ReportItemSet type="String" array="Summer {$last_year}|Fall {$last_year}|Winter {$this_year}|Spring {$this_year}">
            <Static>
                <![CDATA[
                    <h3>{getExtra()}</h3>
                ]]>
            </Static>
            <ReportItemSet type="PersonCourses" term="{removeSpaces({getExtra()})}" unique="true">
                <Static>
                    <![CDATA[
                        <h4 style='padding: 0; margin-top:0; margin-bottom: 0.25em; margin-left:2em; font-size: 1em;'>{$course_subject} {$course_number} - {$course_title}</h4>
                        <div style='margin-left: 4em;'>
                            <b>{getUserSectionCounts()}</b>
                            <table class='wikitable' frame='box' rules='all' style='width:100%;'>
                                <tr>
                                    <th>Percentage of course instruction</th>
                                    <th>Calendar Hours</th>
                                    <th>Total Number of Students</th>
                                </tr>
                                <tr>
                                    <td>{$course_percent}% of instruction</td>
                                    <td align='center'>{$course_calendar}</td>
                                    <td>{getCourseAllLectureEnroll()}</td>
                                </tr>
                            </table><br />
                    ]]>
                </Static>
                <ReportItem id="teaching_methods1" type="Textarea" blobItem="TEACHING_METHODS1" blobSection="FEC_COURSES" height="150px">
                    <![CDATA[
                        <table style='width:100%;'>
                            <tr>
                                <td style='width: 50%;' valign='top'>
                                    <b>(i) Course Design</b><br />
                                </td>
                                <td style='width: 50%;' valign='top'>
                                    <b>(ii) Instructional Practices</b><br />
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    {$item}
                                </td>
                    ]]>
                </ReportItem>
                <ReportItem id="teaching_methods2" type="Textarea" blobItem="TEACHING_METHODS2" blobSection="FEC_COURSES" height="150px">
                    <![CDATA[
                                <td>
                                    {$item}
                                </td>
                            </tr>
                        </table>
                        </div>
                    ]]>
                </ReportItem>
            </ReportItemSet>
        </ReportItemSet>
        
        <If if="{!=({getArray(RP_FEC,FEC_COURSES,FEC_PROJECT_COURSES,0,{$user_id},0,projectCourses)},)}">
            <Static>
                <![CDATA[
                    <h3>Project Courses</h3>
                    <table class="wikitable" frame="box" rules="all" width="100%">
                ]]>
            </Static>
            <ReportItemSet id="projectCourses_array" type="ArrayReportItemSet" array="{getArray(RP_FEC,FEC_COURSES,FEC_PROJECT_COURSES,0,{$user_id},0)}" index="projectCourses">
                <Static>
                    <![CDATA[
                        <tr>
                            <td style="white-space:nowrap;width:10%;">
                                {getExtra(classprefix)} {getExtra(number)}<br />
                                &nbsp;&nbsp;#{getExtra(type)}: {add({occurrences({getExtra(term)},;)},1)}
                            </td>
                            <td width="">{getExtra(title)}<br />{getExtra(description)}</td>
                            <td style="width:45%;"><b>{getExtra(term)}</b>: {add({occurrences({getExtra(term)},;)},1)} {getExtra(type)} ({getExtra(enrolment)})</td>
                        </tr>
                    ]]>
                </Static>
            </ReportItemSet>
            <Static>
                <![CDATA[
                    </table>
                ]]>
            </Static>
        </If>
        
        <ReportItem id="reflection" type="Textarea" blobItem="FEC_COURSES_REFLECTION" blobSection="FEC_COURSES" rich="true" height="150px">
            <![CDATA[
                <h3>Overall Reflection on Teaching</h3>
                {$item}<br />
            ]]>
        </ReportItem>
        
        <!-- HQP Supervision -->
        <ReportItem id="hqpbookmark" type="BookmarkReportItem" title="HQP Supervision">
            <![CDATA[
                <h2>HQP Supervision</h2>
            ]]>
        </ReportItem>
        <ReportItemSet id="hqps" type="String" array="phd|msc|ugrad|pdf|visiting|tech|committee" labels="Doctoral Students|Master's Students|Undergraduates|Post-doctoral Fellows and Research Associates|Visiting Students|Research/Technical Assistants|Student Committee Responsibilities">
            <Static>
                <![CDATA[
                    <h3>{getExtra(label)} ({getHQPCount({getExtra(value)})})</h3>
                    <table class='wikitable' frame='box' rules='all' width='100%'>
                        <tr>
                            <th style='width: 25%;'>Student</th>
                            <th style='width: 10%;'>Year<br />Admitted</th>
                            <th style='width: 25%;'>Degree<br />Sought</th>
                            <th style='width: 10%;'>Year Degree<br />Expected or<br />Completed</th>
                            <th style='width: 20%;'>Co-Supervisors</th>
                        </tr>
                ]]>
            </Static>
            <ReportItemSet type="PersonSupervises" hqpType="{getExtra(value)}">
                <Static>
                    <![CDATA[
                        <tr>
                            <td rowspan='2'>{$user_name}</td>
                    ]]>
                </Static>
                
                <Static>
                    <![CDATA[
                            <td align='center'>{substr({getExtra(start_date)},0,4)}</td>
                            <td>{getExtra(position)}</td>
                            <td align='center'>{substr({getExtra(finish)},0,4)}</td>
                            <td>{$user_cosupervisors}</td>
                    ]]>
                </Static>
                <Static>
                    <![CDATA[
                        </tr>
                    ]]>
                </Static>
                <ReportItem id="scholarships" type="Textarea" blobItem="SCHOLARSHIPS" blobSubItem="{$user_id}" blobSection="FEC_STUDENTS" height="48px">
                    <![CDATA[
                        <tr><td colspan='4'>
                            Name of Scholarships, if any<br />
                            {$item}
                        </td></tr>
                    ]]>
                </ReportItem>
            </ReportItemSet>
            <Static>
                <![CDATA[
                    </table>
                ]]>
            </Static>
        </ReportItemSet>
        
        <!-- Publications -->
        <ReportItem id="publicationbookmark" type="BookmarkReportItem" title="Publications">
            <![CDATA[
                <h2>Publications ({getUserPublicationCount(CYCLE_START, CYCLE_END, AllPublication)})</h2>
                <small>italic = <i>PDF/research associate</i>; bold = <b>graduate student</b>; underline = <u>undergraduate student</u>; light grey/underline = <span style='padding: 1px 3px; border-radius: 3px; background: #dfdfdf; text-decoration: underline;'>the AR owner</span>; light grey = <span style='padding: 1px 3px; border-radius: 3px; background: #dfdfdf;'>other UofA faculty member</span></small>
                <ul>
            ]]>
        </ReportItem>
        <ReportItemSet type="String" array="Journal Paper|
                                            Conference Paper\|Proceedings Paper\|Workshop Paper|
                                            Bachelors Thesis\|Book\|Book Chapter\|Book Review\|Collections Paper\|Edited Book\|Magazine/Newspaper Article\|Masters Thesis\|Misc\|Other\|PHD Thesis\|Journal Abstract\|White Paper\|Tech Report\|Poster\|Manual">
            <ReportItemSet id="products{$index}" type="PersonProductsReportItemSet" category="Publication" productType="{getExtra()}" useProductYear="true" includeHQP="false" start="CYCLE_START" end="CYCLE_END">
                <ReportItem id="row" type="FootnotesReportItem" blobSubItem="{$product_id}" blobSection="FEC_PRODUCTS">
                    <![CDATA[
                        <li style="page-break-inside: avoid;">
                            {$item} 
                    ]]>
                </ReportItem>
                <ReportItem id="annotation" type="AnnotateProductReportItem" blobSection="FEC_PRODUCTS" blobItem="PRODUCT_ANNOTATION" blobType="BLOB_RAW" blobSubItem="{$product_id}" showStatus="true" optional="true">
                    <![CDATA[
                            <b>({$product_type})</b> {$item}
                        </li>
                    ]]>
                </ReportItem>
            </ReportItemSet>
        </ReportItemSet>
        <Static>
            <![CDATA[
                </ul>
            ]]>
        </Static>
        
        <!-- Publication Footnotes -->
        <Static>
            <![CDATA[
                <ul>
            ]]>
        </Static>
        <ReportItemSet id="products1" type="PersonProductsReportItemSet" category="Publication" productType="Journal Paper" useProductYear="true" includeHQP="false" start="CYCLE_START" end="CYCLE_END">
            <ReportItem id="row" type="FootnotesReportItem" blobSubItem="{$product_id}" isTopAnchor="false" blobSection="FEC_PRODUCTS">
                <![CDATA[
                    <li style="page-break-inside: avoid;">
                        {$item}
                    </li>
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItemSet id="products2" type="PersonProductsReportItemSet" category="Publication" productType="Conference Paper|Proceedings Paper|Workshop Paper" useProductYear="true" includeHQP="false" start="CYCLE_START" end="CYCLE_END">
            <ReportItem id="row" type="FootnotesReportItem" blobSubItem="{$product_id}" isTopAnchor="false" blobSection="FEC_PRODUCTS">
                <![CDATA[
                    <li style="page-break-inside: avoid;">
                        {$item}
                    </li>
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItemSet id="products3" type="PersonProductsReportItemSet" category="Publication" productType="Bachelors Thesis|Book|Book Chapter|Book Review|Collections Paper|Edited Book|Magazine/Newspaper Article|Masters Thesis|Misc|PHD Thesis|Proceedings Paper|Journal Abstract|White Paper|Tech Report|Poster|Manual" useProductYear="true" includeHQP="false" start="CYCLE_START" end="CYCLE_END">
            <ReportItem id="row" type="FootnotesReportItem" blobSubItem="{$product_id}" isTopAnchor="false" blobSection="FEC_PRODUCTS">
                <![CDATA[
                    <li style="page-break-inside: avoid;">
                        {$item}
                    </li>
                ]]>
            </ReportItem>
        </ReportItemSet>
        <Static>
            <![CDATA[
                </ul>
            ]]>
        </Static>
        
        <!-- Presentations -->
        <ReportItem id="presentationsbookmark" type="BookmarkReportItem" title="Presentations">
            <![CDATA[
                <h2>Presentations ({getUserPublicationCount(CYCLE_START, CYCLE_END, Presentation)})</h2>
                <table class='wikitable' frame='box' rules='all' width='100%'>
                    <thead>
                        <tr>
                            <th>Title</th>
                            <th>Type</th>
                            <th>Invited</th>
                            <th>Event Title</th>
                            <th>Organization</th>
                            <th>Location</th>
                            <th>Date</th>
                            <th>Length (minutes)</th>
                        </tr>
                    </thead>
            ]]>
        </ReportItem>
        <ReportItemSet id="presentations" type="PersonProductsReportItemSet" category="Presentation" useProductYear="true" includeHQP="false" start="CYCLE_START" end="CYCLE_END">
            <ReportItem id="row" type="FootnotesReportItem" blobSubItem="{$product_id}" blobSection="FEC_PRODUCTS" >
                <![CDATA[
                    <tr><td align='center'>{$presentation_title} <sup>{$item}</sup></td>
                        <td align='center'>{$presentation_type}</td>
                        <td align='center'>{$presentation_invited}</td>
                        <td>{getProductData(event_title)}</td>
                        <td>{$presentation_organization}</td>
                        <td align='center'>{$presentation_country}</td>
                        <td align='center' style='white-space:nowrap;'>{$presentation_date}</td>
                        <td align='right' style='white-space:nowrap;'>{$presentation_length}</td>
                    </tr>
                ]]>
            </ReportItem>
        </ReportItemSet>
        <Static>
            <![CDATA[
                </table>
            ]]>
        </Static>
        
        <!-- Presentations Footnotes -->
        <Static>
            <![CDATA[
                <ul>
            ]]>
        </Static>
        <ReportItemSet id="presentations" type="PersonProductsReportItemSet" category="Presentation" useProductYear="true" includeHQP="false" start="CYCLE_START" end="CYCLE_END">
            <ReportItem id="row" type="FootnotesReportItem" blobSubItem="{$product_id}" isTopAnchor="false"  blobSection="FEC_PRODUCTS">
                <![CDATA[
                    <li style="page-break-inside: avoid;">
                        {$item}
                    </li>
                ]]>
            </ReportItem>
        </ReportItemSet>
        <Static>
            <![CDATA[
                </ul>
            ]]>
        </Static>
        
        <!-- Patents/Spin-Offs -->
        <ReportItem id="patentsbookmark" type="BookmarkReportItem" title="Patents/Spin-Offs">
            <![CDATA[
                <h2>Patents/Spin-Offs ({getUserPublicationCount(CYCLE_START, CYCLE_END, Patent/Spin-Off)})</h2>
                <ul>
            ]]>
        </ReportItem>
        <ReportItemSet id="patents" type="PersonProductsReportItemSet" category="Patent/Spin-Off" useProductYear="true" includeHQP="false" start="CYCLE_START" end="CYCLE_END">
            <ReportItem id="row" type="FootnotesReportItem" blobSubItem="{$product_id}" blobSection="FEC_PRODUCTS" >
                <![CDATA[
                    <li style="page-break-inside: avoid;">{$item}
                ]]>
            </ReportItem>
            <ReportItem id="annotation" type="AnnotateProductReportItem" blobSection="FEC_PRODUCTS" blobItem="PRODUCT_ANNOTATION" blobType="BLOB_RAW" blobSubItem="{$product_id}" showStatus="true" optional="true">
                <![CDATA[
                        <b>({$product_type})</b> {$item}
                    </li>
                ]]>
            </ReportItem>
        </ReportItemSet>
        <Static>
            <![CDATA[
                </ul>
            ]]>
        </Static>
        
        <!-- Patents Footnotes -->
        <Static>
            <![CDATA[
                <ul>
            ]]>
        </Static>
        <ReportItemSet id="patents" type="PersonProductsReportItemSet" category="Patent/Spin-Off" useProductYear="true" includeHQP="false" start="CYCLE_START" end="CYCLE_END">
            <ReportItem id="row" type="FootnotesReportItem" blobSubItem="{$product_id}" isTopAnchor="false"  blobSection="FEC_PRODUCTS">
                <![CDATA[
                    <li style="page-break-inside: avoid;">
                        {$item}
                    </li>
                ]]>
            </ReportItem>
        </ReportItemSet>
        <Static>
            <![CDATA[
                </ul>
            ]]>
        </Static>
        
        <!-- Awards Received -->
        <ReportItem id="awardsbookmark" type="BookmarkReportItem" title="Awards Received">
            <![CDATA[
                <h2>Awards Received ({getUserPublicationCount(CYCLE_START, CYCLE_END, Award)})</h2>
                <table class='wikitable' frame='box' rules='all' width='100%'>
                    <thead>
                        <tr>
                            <th>Award</th>
                            <th>Award Category</th>
                            <th>Scope</th>
                            <th>Year</th>
                        </tr>
                    </thead>
