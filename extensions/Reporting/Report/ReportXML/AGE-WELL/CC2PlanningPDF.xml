            <![CDATA[
                <h4>How will you measure the effectiveness of your commercialization activities?</h4>
                {$item}<br />
            ]]>
        </ReportItem>
        <ReportItem id="support" type="TextareaReportItem" blobItem="CC_2_SUPPORT">
