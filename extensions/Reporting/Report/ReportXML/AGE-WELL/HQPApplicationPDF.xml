<?xml version="1.0" encoding="UTF-8" ?>
<Report name="Award 2017" headerName="{$user_name}: {$hqp_application_uni} / {$hqp_application_program}" reportType="RP_HQP_APPLICATION" pdfType="RPTP_HQP_APPLICATION" ajax="true" disabled="false" year="2017">
    <ReportSection id="status" name="Header" type="HeaderReportSection" pagebreak="false" renderpdf="true">
