        <ReportItem id="NBHRF" type="CheckboxReportItem" blobType="BLOB_ARRAY" blobItem="HQP_APPLICATION_NBHRF" orientation="vertical" options="Master's ($17,500)|PhD ($25,000 per year for 2 years)|Postdoctoral, PhD ($40,000)|Postdoctoral, MD or Pharm. D ($56,500 for 2 years)" optional="true">
