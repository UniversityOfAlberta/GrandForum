            <![CDATA[
                <h4>Applicant Name</h4>
                <p>{$user_name}</p><br />
            ]]>
        </ReportItem>
        <ReportItem id="status" type="TextReportItem" blobType="BLOB_TEXT" blobItem="HQP_APPLICATION_STAT">
