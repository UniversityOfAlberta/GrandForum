                <h4>Academic status and Department</h4>
                <p>{$item}</p><br />
            ]]>
        </ReportItem>
        <ReportItem id="supervisor" type="TextReportItem" blobType="BLOB_TEXT" blobItem="HQP_APPLICATION_SUP">
