                <![CDATA[
                    <tr>
                        <td>{$user_name}</td>
                ]]>
            </Static>
            <ReportItem id="scholarships" type="Textarea" blobItem="SCHOLARSHIPS" blobSubItem="{$user_id}">
                <![CDATA[
                    <td>{$item}</td>
                ]]>
            </ReportItem>
            <ReportItem id="time" type="Select" blobItem="TIME" blobSubItem="{$user_id}" options="FT|PT">
                <![CDATA[
                    <td align='center'>{$value}</td>
                ]]>
            </ReportItem>
            <Static>
                <![CDATA[
                    <td align='center'>{substr({$user_uni_start},-4,4)}</td>
                    <td>{$user_level}</td>
                    <td align='center'>{substr({$user_uni_end},-4,4)}</td>
