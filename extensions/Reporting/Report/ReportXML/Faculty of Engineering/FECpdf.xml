                    </table>
                </div>
            ]]>
        </Static>
        
        <Static>
            <![CDATA[
                <h2>Supplementary Professional Activities</h2>
            ]]>
        </Static>
        <ReportItem id="spa_level" type="SelectReportItem" blobItem="FEC_SPA_LEVEL" blobType="BLOB_TEXT" options="None|Minor SPA|Major SPA">
            <![CDATA[
                <h3>If you have SPA to report, select "Minor SPA" or "Major SPA"</h3>
                {$item}
            ]]>
        </ReportItem>
        <If if="{or({==({getText(RP_FEC,FEC_COVER,FEC_SPA_LEVEL,0,{$user_id},0)},Minor SPA)},
                        {==({getText(RP_FEC,FEC_COVER,FEC_SPA_LEVEL,0,{$user_id},0)},Major SPA)})}">
            <ReportItem id="spa_disclosure" type="RadioReportItem" blobItem="FEC_SPA_DISCLOSURE" blobType="BLOB_TEXT" options="Yes|No">
                <![CDATA[
                    <h3>I confirm that I have an up to date Disclosure Report on file</h3>
                    {$item}
                ]]>
            </ReportItem>
            <If if="{==({getText(RP_FEC,FEC_COVER,FEC_SPA_LEVEL,0,{$user_id},0)},Major SPA)}">
                <ReportItem id="spa_major" type="RadioReportItem" blobItem="FEC_SPA_MAJOR" blobType="BLOB_TEXT" options="Yes|No">
                    <![CDATA[
                        <h3>I confirm that I have discussed my Major SPA with my Department Chair and obtained written permission prior to engaging in the SPA</h3>
                        {$item}
                    ]]>
                </ReportItem>
            </If>
            <ReportItem id="spa_description" type="TextareaReportItem" blobItem="FEC_SPA_DESC" blobType="BLOB_TEXT">
                <![CDATA[
                    <h3>Describe the SPA below</h3>
                    {$item}
                ]]>
            </ReportItem>
        </If>
        <If if="{or({==({getText(RP_FEC,FEC_COVER,FEC_SPA_LEVEL2,0,{$user_id},0)},Minor SPA)},
                        {==({getText(RP_FEC,FEC_COVER,FEC_SPA_LEVEL2,0,{$user_id},0)},Major SPA)})}">
            <ReportItem id="spa_level2" type="SelectReportItem" blobItem="FEC_SPA_LEVEL2" blobType="BLOB_TEXT" options="None|Minor SPA|Major SPA">
                <![CDATA[
                    <br />
                    <h3>Additional SPA</h3>
                    {$item}
                ]]>
            </ReportItem>
            <ReportItem id="spa_disclosure2" type="RadioReportItem" blobItem="FEC_SPA_DISCLOSURE2" blobType="BLOB_TEXT" options="Yes|No">
                <![CDATA[
                    <h3>I confirm that I have an up to date Disclosure Report on file</h3>
                    {$item}
                ]]>
            </ReportItem>
            <If if="{==({getText(RP_FEC,FEC_COVER,FEC_SPA_LEVEL2,0,{$user_id},0)},Major SPA)}">
                <ReportItem id="spa_major2" type="RadioReportItem" blobItem="FEC_SPA_MAJOR2" blobType="BLOB_TEXT" options="Yes|No">
                    <![CDATA[
                        <h3>I confirm that I have discussed my Major SPA with my Department Chair and obtained written permission prior to engaging in the SPA</h3>
                        {$item}
                    ]]>
                </ReportItem>
            </If>
            <ReportItem id="spa_description2" type="TextareaReportItem" blobItem="FEC_SPA_DESC2" blobType="BLOB_TEXT">
                <![CDATA[
                    <h3>Describe the SPA below</h3>
                    {$item}
                ]]>
            </ReportItem>
        </If>
    </ReportSection>
    <ReportSection id="teaching" tooltip="TEACHING" name="Teaching" title="1. TEACHING" blobSection="FEC_TEACHING" type="EditableReportSection" pagebreak="false" hLevel="2">
        <!-- First Term of Last Academic Year -->
        <Static>
            <![CDATA[
                <h3>Fall {$last_year}</h3>
            ]]>
        </Static>
        <ReportItemSet type="PersonCourses" term="Fall{$last_year}" component="LEC" unique="true">
            <Static>
                <![CDATA[
                    <h4 style='padding: 0; margin-top:0; margin-bottom: 0.25em; margin-left:2em; font-size: 1em;'>{$course_subject} {$course_number} - {$course_title}</h4>
                    <div style='margin-left: 4em;'>
                        <table class='wikitable' frame='box' rules='all' style='width:100%;'>
                            <tr>
                                <th>Percentage of course instruction</th>
                                <th>Calendar Hours</th>
                                <th>Actual Number of Students</th>
                            </tr>
                            <tr>
                                <td>{$course_percent}% of instruction</td>
                                <td align='center'>{$course_calendar}</td>
                                <td>{$course_enroll}</td>
                            </tr>
                        </table><br />
                ]]>
            </Static>
            <If if="{!=({$course_eval}, )}">
                <![CDATA[
                    <table class='wikitable' frame='box' rules='all' style='width:100%;'>
                        <tr>
                            <th style='width: 1%;'>Q#</th>
                            <th style='width: 1%;'>QText</th>
                            <th style='width: 1%;'>R1</th>
                            <th style='width: 1%;'>R2</th>
                            <th style='width: 1%;'>R3</th>
                            <th style='width: 1%;'>R4</th>
                            <th style='width: 1%;'>R5</th>
                            <th>Chart</th>
                        </tr>
                        {$course_eval}
                    </table><br />
                ]]>
            </If>
            <ReportItem id="teaching_methods" type="MultiText" blobItem="TEACHING_METHODS" blobType="BLOB_ARRAY" labels="Short Description and Educational Objective|Number of hours invested|Method used to evaluate initiative" types="Textarea|Number|Textarea" sizes="62.60%|10%|27.40%" class="wikitable">
                <![CDATA[
                        {$item}
                    </div>
                ]]>
            </ReportItem>
        </ReportItemSet>
        
        <!-- Second Term of Last Academic Year -->
        <Static>
            <![CDATA[
                <h3>Winter {$this_year}</h3>
            ]]>
        </Static>
