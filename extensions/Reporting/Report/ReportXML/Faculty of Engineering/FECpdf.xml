                            <th style='width: 1%;'>Q#</th>
                            <th style='width: 1%;'>QText</th>
                            <th style='width: 1%;'>R1</th>
                            <th style='width: 1%;'>R2</th>
                            <th style='width: 1%;'>R3</th>
                            <th style='width: 1%;'>R4</th>
                            <th style='width: 1%;'>R5</th>
                            <th>Chart</th>
