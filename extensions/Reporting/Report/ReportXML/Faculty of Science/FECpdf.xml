<?xml version="1.0" encoding="UTF-8" ?>
<Report name="FEC" reportType="RP_FEC" pdfType="RPTP_FEC" ajax="true">
    <ReportSection id="information" tooltip="Information" name="Information" title="Annual Report of Faculty Member" blobSection="FEC_INFORMATION" type="EditableReportSection">
        <ReportItem id="information" type="StaticReportItem">
            <![CDATA[
            <style>
            	table.one{
           	    border-collapse:collapse;
            	    width:100%;
            	}
            	td.b{
            	    border: 1px solid black;
            	}
            }
            </style>
            	<table><tr><td style='width:50%'>
            	<table class='wikitable'>
            	<tr><td>Name:</td><td><b>{$user_name}</b></td></tr>
                <tr><td>Rank:</td><td><b>{$user_level}</b></td></tr>
                <tr><td>Department</td><td><b>{$user_dept}</b></td></tr>
            ]]>
        </ReportItem>
        <ReportItem id="phd" type="IntegerReportItem" min="0" max="{$this_year}" width="50px" blobItem="FEC_INFO_PHD" blobType="BLOB_TEXT">
	    <![CDATA[
		<tr><td>Year of PhD:</td><td><b>{$item}</b></td></tr>
	    ]]>
	</ReportItem>
        <ReportItem id="appointment" type="IntegerReportItem" min="0" max="{$this_year}" width="50px" blobItem="FEC_INFO_APPOINTMENT" blobType="BLOB_TEXT">
             <![CDATA[
                 <tr><td>Year of Appointment:&nbsp;</td><td><b>{$item}</b></td></tr>
             ]]>
        </ReportItem>
        <ReportItem id="gradCount" type="StaticReportItem">
            <![CDATA[
            	<tr><td>Graduate Students(Supervised or Co-supervised):</td><td><b>{$user_grad_count}</td></b></tr>
                <tr><td style='width:350px;'>Post-doctoral Fellows and Research Associates (Supervised or Co-supervised):</td><td><b>{$user_fellow_count}</b></tr></td>
	    	</table></td><td><div>
	    	    <center><b>Period: July {subtract({$this_year},1)} to June {$this_year}</b></center>A staff member is required to submit an annual report covering the staff member's activities during the previous year; see Faculty Agreement or FSO Agreement. The information submited will be used in arriving at decisions regrading salaries, tenure, and promotion.</center></div>
		<table width:'100%'>
		    <tr>
		        <th></th>
		    	<th>Refereed</th>
		    	<th>Books</th>
		    	<th>Patents</th>
		    </tr>
		    <tr>
		        <td><b>{subtract({$this_year},1)}/{$this_year}</b></td>
		        <td align='center'>{getUserPublicationCount({subtract({$this_year},1)}-01-01, {$this_year}-12-31)}</td>
		        <td align='center'>{getUserPublicationCount({subtract({$this_year},1)}-01-01, {$this_year}-12-31,Book)}</td>
		        <td align='center'>{getUserPublicationCount({subtract({$this_year},1)}-01-01, {$this_year}-12-31,Patent)}</td>
		    </tr>
		    <tr>
		        <td><b>{subtract({$this_year},2)}/{subtract({$this_year},1)}</b></td>
		        <td align='center'>{getUserPublicationCount({subtract({$this_year},2)}-01-01, {subtract({$this_year},1)}-12-31)}</td>
		        <td align='center'>{getUserPublicationCount({subtract({$this_year},2)}-01-01, {subtract({$this_year},1)}-12-31, Book)}</td>
		        <td align='center'>{getUserPublicationCount({subtract({$this_year},2)}-01-01, {subtract({$this_year},1)}-12-31, Patent)}</td>
		    </tr>
		    <tr>
		    	<td><b>{subtract({$this_year},3)}/{subtract({$this_year},2)}</b></td>
		    	<td align='center'>{getUserPublicationCount({subtract({$this_year},3)}-01-01, {subtract({$this_year},2)}-12-31)}</td>
		    	<td align='center'>{getUserPublicationCount({subtract({$this_year},3)}-01-01, {subtract({$this_year},2)}-12-31, Book)}</td>
		    	<td align='center'>{getUserPublicationCount({subtract({$this_year},3)}-01-01, {subtract({$this_year},2)}-12-31, Patent)}</td>
		    </tr>
		    <tr>
		    	<td><b>{subtract({$this_year},4)}/{subtract({$this_year},3)}</b></td>
		    	<td align='center'>{getUserPublicationCount({subtract({$this_year},4)}-01-01, {subtract({$this_year},3)}-12-31)}</td>
		    	<td align='center'>{getUserPublicationCount({subtract({$this_year},4)}-01-01, {subtract({$this_year},3)}-12-31,Book)}</td>
		    	<td align='center'>{getUserPublicationCount({subtract({$this_year},4)}-01-01, {subtract({$this_year},3)}-12-31,Patent)}</td>
		    </tr>
		    <tr>
		    	<td><b>{subtract({$this_year},5)}/{subtract({$this_year},4)}</b></td>
		    	<td align='center'>{getUserPublicationCount({subtract({$this_year},5)}-01-01, {subtract({$this_year},4)}-12-31)}</td>
		    	<td align='center'>{getUserPublicationCount({subtract({$this_year},4)}-01-01, {subtract({$this_year},3)}-12-31,Book)}</td>
		    	<td align='center'>{getUserPublicationCount({subtract({$this_year},4)}-01-01, {subtract({$this_year},3)}-12-31,Patent)}</td>
		    </tr>
		    <tr>
		    	<td><b>Lifetime</b></td>
		    	<td align='center'>{user_lifetime_pubs_count(Publication)}</td>
		    	<td align='center'>{user_lifetime_pubs_count(Book)}</td>
		    	<td align='center'>{user_lifetime_pubs_count(Patent)}</td>
		    </tr>
		</table>
		</td></tr></table>
	    ]]>
	</ReportItem>
	<ReportItem id="responsibilities" type="RadioReportItem" blobItem="FEC_INFO_RESP" blobType="BLOB_TEXT" options="Yes|No">
	    <![CDATA[
	    	<h2>Responsibilities Variation</h2>
	    	Is there a formal Variance of Responsibilities Agreement in place as per Articles 7.02.2 and 13.1 of the Faculty Agreement?
	    	<div style='margin-left:25px;'>{$item}</div>
	    ]]>
	</ReportItem>
	<ReportItem id="leaves" type="MultiTextReportItem" blobItem="FEC_INFO_LEAVES" blobType="BLOB_ARRAY" labels="Category|Start Date|End Date" types="Text|Date(yy/mm)|Date(yy/mm)" multiple="true" optional="true" class="wikitable">
	    <![CDATA[
		<h2>Leaves</h2>
		{$item}
	    ]]>
	</ReportItem>
	<ReportItem id="reasons" type="TextareaReportItem" blobItem="FEC_INFO_REASONS" blobType="BLOB_TEXT" rich="true" optional="true">
	    <![CDATA[
		<h2>Reasons For Reduced Teaching<small> (if applicable)</small></h2>
		{$item}
	    ]]>
	</ReportItem>
        <ReportItem id="development" type="TextareaReportItem" blobItem="FEC_INFO_DEVELOPMENT" blobType="BLOB_TEXT" rich="true" optional="true">
            <![CDATA[
                <h2>Teaching Development And Innovation</h2>
                  {$item}
            ]]>
        </ReportItem>
	<ReportItem id="coursesheader" type="StaticReportItem">
	    <![CDATA[
		<h2>Courses</h2>
		<table class="one"><thead><tr><th>Term</th>
		<th>Subject</th>
		<th>Number</th>
		<th>Comp.</th>
		<th>Section</th>
		<th>Enroll.</th>
		<th>%</th></tr></thead><tbody>
	    ]]>
	</ReportItem>
	<ReportItemSet id="courses" type="PersonCoursesReportItemSet">
	     <ReportItem id="row" type="StaticReportItem">
	     	<![CDATA[

	             <tr><td class="b" align='center'>{$course_term}</td>
	             <td class="b" align='center'>{$course_subject}</td>
                     <td class="b" align='center'>{$course_number}</td>
                     <td class="b" align='center'>{$course_comp}</td>
                     <td class="b" align='center'>{$course_section}</td>
                     <td class="b" align='center'>{$course_enroll}</td>
	             <td class="b" align='center'>{$course_enroll_percent}</td></tr>                         

            	]]>
	     </ReportItem>
	</ReportItemSet>
	<ReportItem id="coursesfooter" type="StaticReportItem">
	    <![CDATA[
	    	</tbody></table>
            ]]>
        </ReportItem>
        <ReportItem id="otherTeaching" type="TextareaReportItem" blobItem="FEC_INFO_TEACHING" blobSection="FEC_COURSES" blobType="BLOB_TEXT" rich="true" optional="true">
            <![CDATA[
                <h2>Other Teaching</h2>{$item}
            ]]>
        </ReportItem>
    	<ReportItem id="studentsheader" type="StaticReportItem">
    	    <![CDATA[
    	    	<h2>Responsibilities for HQP</h2>
    	    	<table class='one'><thead><tr>
    	    	    <th>Name</th>
    	    	    <th>Resp</th>
    	    	    <th>Awards</th>
    	    	    <th>Role</th>
    	    	    <th>Start Date</th>
    	    	    <th>End Date</th>
    	    	    <th>Status</th></tr></thead><tbody>
    	    ]]>
    	 </ReportItem>
    	 <ReportItemSet id="students" type="PersonSupervisesRelationReportItemSet">
    	     <ReportItem id="row" type="StaticReportItem">
    	         <![CDATA[
    	         	<tr><td class="b">{$hqp_name}</td>
    	         	<td class="b" align='center'>{$hqp_position}</td>
    	         	<td class="b" align='center'>{$hqp_awards}</td>
    	         	<td class="b" align='center'>{$user_hqp_role}</td>
    	         	<td class="b" align='center'>{$hqp_start_date}</td>
    	         	<td class="b" align='center'>{$hqp_end_date}</td>
    	         	<td class="b" align='center'>{$hqp_status}</td></tr>
    	         ]]>
    	     </ReportItem>
    	 </ReportItemSet>
    	 <ReportItem id="studentsfooter" type="StaticReportItem">
    	     <![CDATA[
    	         </tbody></table>
    	     ]]>
         </ReportItem>
        <ReportItem id="productsheader" type="StaticReportItem">
            <![CDATA[
		<h2>Publications</h2>
		<table class='wikitable' style='width:100%'><thead><tr>
		<th>Refereed Type Authors Title Venue Accepted Published Pg. Eds. Vol. Issue Place Publisher DOI URL</th></tr></thead><tbody>
	    ]]>
        </ReportItem>
        <ReportItemSet id="products" type="PersonProductsReportItemSet" category="Publication">
	    <ReportItem id="row" type="StaticReportItem">
	         <![CDATA[
		     <tr><td>{$product_citation}</td></tr>
		 ]]>
	    </ReportItem>
        </ReportItemSet>
        <ReportItem id="productsfooter" type="StaticReportItem">
            <![CDATA[
                </tbody></table>
            ]]>
        </ReportItem>
        <ReportItem id="presentationheader" type="StaticReportItem">
	    <![CDATA[
                <h2>Presentations</h2>
                <table class='one' style='width:100%'><tr>
                	<th>Category</th>
                	<th>Invited</th>
                	<th>Refereed</th>
                	<th>Organization</th>
                	<th>Country</th>
                	<th>Date</th>
                	<th>Minutes</th></tr>
            ]]>
        </ReportItem>
        <ReportItemSet id="presentations" type="PersonPresentationReportItemSet">
            <ReportItem id="row" type="StaticReportItem">
                <![CDATA[
                    <tr><td align='center' class="b">Presentation</td>
                    	<td align='center' class="b">{$presentation_invited}</td>
                    	<td align='center' class="b">{$presentation_refereed}</td>
                    	<td class="b">{$presentation_organization}</td>
                    	<td align='center' class="b">{$presentation_country}</td>
                    	<td align='center' style='white-space:nowrap;' class="b">{$presentation_date}</td>
                    	<td align='center' class="b">{$presentation_duration}</td>
                    	</tr>
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItem id="presentationfooter" type="StaticReportItem">
            <![CDATA[
                </table>
            ]]>
        </ReportItem>
        <ReportItem id ="grantsheader" type="StaticReportItem">
            <![CDATA[
		<h2>Research Grants and Contracts</h2>
		<table class='one'><thead><tr>
		<th>Scope</th>
		<th>Agency</th>
		<th>Program</th>
		<th>Starts</th>
		<th>Ends</th>
		<th>$Yearly</th>
		<th>$Total</th>
		<th>PI</th>
		<th>Co-recipients</th></tr></thead><tbody>
	    ]]>
	</ReportItem> 
	<ReportItemSet id="grants" type="PersonContributionsReportItemSet">
	    <ReportItem id="row" type="StaticReportItem">
	        <![CDATA[
	            <tr>
	            	<td class='b' align='center'>{$contribution_scope}</td>
	            	<td class='b' align='center'>{$contribution_agency}</td>
	            	<td class='b' align='center'>{$contribution_program}</td>
	            	<td class='b' align='center' style='white-space:nowrap;'>{$contribution_start_date}</td>
	            	<td class='b' align='center' style='white-space:nowrap;'>{$contribution_end_date}</td>
	            	<td class='b' align='right'>${$contribution_yearly}</td>
	            	<td class='b' align='right'>${$contribution_total}</td>
	            	<td class='b'>{$contribution_pis}</td>
	            	<td class='b'>{$contribution_recipients}</td>
	            </tr>
	        ]]>
	    </ReportItem>
	</ReportItemSet>
	<ReportItem id="grantsfooter" type="StaticReportItem">
	    <![CDATA[
	        </tbody></table>
	    ]]>
	</ReportItem>
	<ReportItem id="awardsReceived" type="MultiTextReportItem" blobItem="FEC_INFO_AWARDS" blobType="BLOB_ARRAY" labels=
	"Name|Category|Scope" types="Text|Text|Text" multiple="true" optional="true" blobSection="FEC_GRANTS">
	    <![CDATA[
	        <h2>Awards Received</h2>{$item}
	    ]]>
	</ReportItem>
        <ReportItem id="profActivities" type="TextareaReportItem" blobItem="FEC_INFO_ACTIVITIES" blobType="BLOB_TEXT" rich="true" optional="true" blobSection="FEC_ADDITIONAL_INFO">
            <![CDATA[
            	<h2>Supplementary Professional Activities</h2>{$item}
            ]]>
        </ReportItem>
        <ReportItem id="serviceOutreachDepartmental" type="TextareaReportItem" blobItem="FEC_INFO_DEPARTMENTAL" blobType="BLOB_TEXT" rich="true" optional="true" blobSection="FEC_ADDITIONAL_INFO">
            <![CDATA[
            	<h2>Service/Committees/Outreach</h2><b>Departmental</b><div>{$item}</div><br>
            ]]>
        </ReportItem>
        <ReportItem id="serviceOutreachFaculty" type="TextareaReportItem" blobItem="FEC_INFO_FACULTY" blobType="BLOB_TEXT" rich="true" optional="true" blobSection="FEC_ADDITIONAL_INFO">
            <![CDATA[
                <b>Faculty</b><div>{$item}</div><br>
            ]]>
        </ReportItem>
        <ReportItem id="serviceOutreachUniversity" type="TextareaReportItem" blobItem="FEC_INFO_UNIVERSITY" blobType="BLOB_TEXT" rich="true" optional="true" blobSection="FEC_ADDITIONAL_INFO">
            <![CDATA[
                <b>University</b><div>{$item}</div><br>
            ]]>
        </ReportItem>
        <ReportItem id="serviceOutreachScientific" type="MultiTextReportItem" blobItem="FEC_INFO_SCIENTIFIC" blobType="BLOB_ARRAY" labels="Scope|Organization|Description" types="Text|Text|Text" multiple="true" optional="true" blobSection="FEC_ADDITIONAL_INFO" class="wikitable">
            <![CDATA[
                <b>Scientific</b><div>{$item}</div>
            ]]>
        </ReportItem>
        <ReportItem id="serviceOutreachCommunity" type="TextareaReportItem" blobItem="FEC_INFO_COMMUNITY" blobType="BLOB_TEXT" rich="true" optional="true" blobSection="FEC_ADDITIONAL_INFO">
            <![CDATA[
                <b>Outreach/Community Engagement/Media</b><div>{$item}</div><br>
            ]]>
        </ReportItem>
        <ReportItem id="serviceOutrechOther" type="TextareaReportItem" blobItem="FEC_INFO_OTHER" blobType="BLOB_TEXT" rich="true" optional="true" blobSection="FEC_ADDITIONAL_INFO">
            <![CDATA[
            	<b>Other</b>{$item}<br>
            ]]>
        </ReportItem>
        <ReportItem id="additionalData" type="TextareaReportItem" blobItem="FEC_INFO_ADDITIONAL" blobType="BLOB_TEXT" blobSection="FEC_ADDITIONAL_INFO">
            <![CDATA[
                <h2>Additional Data</h2>{$item}
            ]]>
        </ReportItem>
        <ReportItem id="footnotes" type="TextareaReportItem" blobItem="FEC_INFO_FOOTNOTES" blobType="BLOB_TEXT" rich="true" optional="true" blobSection="FEC_ADDITIONAL_INFO">
            <![CDATA[
            	<b>Footnotes</b>{$item}
            ]]>
        </ReportItem>
    </ReportSection>
</Report>
