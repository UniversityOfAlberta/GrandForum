<?xml version="1.0" encoding="UTF-8" ?>
<Products>

    <!-- Publications -->
    <Publications category="Publication" citationFormat="{%Authors} {(%Dates).} {%Title.} {&lt;i>%Venue&lt;/i>}{, %Volume}{(%Issue)}{ #%Articleno}{, %Pages.} {%Publisher}&lt;br />{DOI: %doi}">
        
    </Publications>

    <!-- Patents and Spin Offs -->
    <Products category="Patent/Spin-Off" citationFormat="{%Authors} {(%YYYY %Mon).} {%Title.} {#%Issue.} {Place: %Venue}">
        <Product type="ICT Filed" status="">
            <data>
                <field label="Publisher" type="String">publisher</field>
                <field label="URL" type="URL">url</field>
            </data>
            <authors label="Author"></authors>
            <date label="Date"></date>
        </Product>
        
        <Product type="Provisional Patent Filed" citationFormat="&lt;b>{%Status}&lt;/b> - {%Authors} {(%YYYY %Mon).} {%Title.} {#%Issue.} {Place: %Venue}">
            <data>
                <field bibtex="number" label="Number" type="String">number</field>
                <field label="Country" type="String">country</field>
                <field label="Tech Department" type="String">tech_department</field>
                <field bibtex="url" label="URL" type="URL">url</field>
            </data>
            <description>A patent or patent application.</description>
            <authors label="Inventor"></authors>
            <acceptance_date label="Date Filed"></acceptance_date>
            <date label="Date Awarded"></date>
        </Product>
        
        <Product type="Non-Provisional Patent Filed" citationFormat="&lt;b>{%Status}&lt;/b> - {%Authors} {(%YYYY %Mon).} {%Title.} {#%Issue.} {Place: %Venue}">
            <data>
                <field bibtex="number" label="Number" type="String">number</field>
                <field label="Country" type="String">country</field>
                <field label="Tech Department" type="String">tech_department</field>
                <field bibtex="url" label="URL" type="URL">url</field>
            </data>
            <description>A patent or patent application.</description>
            <authors label="Inventor"></authors>
            <acceptance_date label="Date Filed"></acceptance_date>
            <date label="Date Awarded"></date>
        </Product>
        
        <Product type="Patent Allowed" citationFormat="&lt;b>{%Status}&lt;/b> - {%Authors} {(%YYYY %Mon).} {%Title.} {#%Issue.} {Place: %Venue}">
            <data>
                <field bibtex="number" label="Number" type="String">number</field>
                <field label="Country" type="String">country</field>
                <field label="Tech Department" type="String">tech_department</field>
                <field bibtex="url" label="URL" type="URL">url</field>
            </data>
            <description>A patent or patent application.</description>
            <authors label="Inventor"></authors>
            <acceptance_date label="Date Filed"></acceptance_date>
            <date label="Date Awarded"></date>
        </Product>
        
        <Product type="Patent Issued" citationFormat="&lt;b>{%Status}&lt;/b> - {%Authors} {(%YYYY %Mon).} {%Title.} {#%Issue.} {Place: %Venue}">
            <data>
                <field bibtex="number" label="Number" type="String">number</field>
                <field label="Country" type="String">country</field>
                <field label="Tech Department" type="String">tech_department</field>
                <field bibtex="url" label="URL" type="URL">url</field>
            </data>
            <description>A patent or patent application.</description>
            <authors label="Inventor"></authors>
            <acceptance_date label="Date Filed"></acceptance_date>
            <date label="Date Awarded"></date>
        </Product>
        
        <Product type="Patent Licenced" citationFormat="&lt;b>{%Status}&lt;/b> - {%Authors} {(%YYYY %Mon).} {%Title.} {#%Issue.} {Place: %Venue}">
            <data>
                <field bibtex="number" label="Number" type="String">number</field>
                <field label="Country" type="String">country</field>
                <field label="Tech Department" type="String">tech_department</field>
                <field bibtex="url" label="URL" type="URL">url</field>
            </data>
            <description>A patent or patent application.</description>
            <authors label="Inventor"></authors>
            <acceptance_date label="Date Filed"></acceptance_date>
            <date label="Date Awarded"></date>
        </Product>
    </Products>  

    <Contributions category="Contribution" citationFormat="{%Authors} {(%Range).} {%Title.} {&lt;i>%Venue&lt;/i>}{, %Volume}{(%Issue)}{:%Pages.} {%Publisher}">
        <Contribution type="Teaching/Pedagogy">
             <data>
             </data>
             <authors label="Individual Involved"></authors>
             <acceptance_date label="Start Date"></acceptance_date>
             <date label="End Date"></date>
        </Contribution>
        
        <Contribution type="Research/Professional Practice">
             <data>
             </data>
             <authors label="Individual Involved"></authors>
             <acceptance_date label="Start Date"></acceptance_date>
             <date label="End Date"></date>
        </Contribution>
        
        <Contribution type="Technical Committee/Board">
             <data>
                <field label="Technical Committee/Board">committee</field>
             </data>
             <authors label="Individual Involved"></authors>
             <acceptance_date label="Start Date"></acceptance_date>
             <date label="End Date"></date>
        </Contribution>
        
        <Contribution type="Other">
             <data>
             </data>
             <authors label="Individual Involved"></authors>
             <acceptance_date label="Start Date"></acceptance_date>
             <date label="End Date"></date>
        </Contribution>
    </Contributions>

</Products>
